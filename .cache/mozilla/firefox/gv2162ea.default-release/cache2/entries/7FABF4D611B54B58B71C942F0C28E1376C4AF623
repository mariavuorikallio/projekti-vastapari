if (p!=null) {
delete hX_6.action.pBlocks[p];
hX_6.action.pBlocks[p]=null;
while (hX_6.action.pBlocks.length > 0 && hX_6.action.pBlocks[hX_6.action.pBlocks.length-1]==null) hX_6.action.pBlocks.pop();
}
}
} else {
for (i=0; p==null && i<hX_6.action.pBlocks.length; i++) if (hX_6.action.pBlocks[i]!=null && hX_6.action.pBlocks[i].id==ID) p=i;
