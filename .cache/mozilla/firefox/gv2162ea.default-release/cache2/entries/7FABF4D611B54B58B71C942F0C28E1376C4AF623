this._2chars="gAyYGMEDSdhHmskKw";
this.FMTSEPChars="-_ .,;:|()<>/\\";
this.SEPChars=this.FMTSEPChars;
this.bIsGregorian=true;
this.bIsHijri=this.bIsHebrew=this.bIsChristianEra=this.bIsBuddhistEra=this.bIsJapaneseEra=false;
