if (c1 == pChar || c1 == hX_6._B) c1=hX_6._2;
if (c2 == pChar || c2 == hX_6._B) c2=hX_6._Z;
if (c3 == pChar || c3 == hX_6._B) c3=hX_6._Z;
if (c4 == pChar || c4 == hX_6._B) c4=hX_6._Z;
