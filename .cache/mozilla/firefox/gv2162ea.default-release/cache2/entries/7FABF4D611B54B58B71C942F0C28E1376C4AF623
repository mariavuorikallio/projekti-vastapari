obj.onmouse=fcn;
break;
case hX_6._onPK:
obj.onpoke=fcn;
break;
case hX_6._onPC:
obj.onpropertychange=fcn;
break;
case hX_6._onDOMPC:
obj.DOMAttrModified=fcn;
break;
case hX_6._onDOMVPC:
obj.DOMCharacterDataModified=fcn;
break;
case hX_6._onR:
obj.onresize=fcn;
break;
case hX_6._onF:
obj.onfocus=fcn;
break;
