function isFilled (curpos) {
var l=elem.value.length;
if (l < cvt.maskFormat.length)
return false;
var c=elem.value.charAt(cvt.maskFormatLast);
return (curpos > cvt.maskFormatLast && c != pChar && c != hX_6._B);
}
function onFocus (evt) {
var newValue=toAssist(elem.value);
elem.value=(newValue.v==null) ? hX_6._E : newValue.v;
hX_6.sel.setCaretPos(elem, cvt.maskFormatFirst);
