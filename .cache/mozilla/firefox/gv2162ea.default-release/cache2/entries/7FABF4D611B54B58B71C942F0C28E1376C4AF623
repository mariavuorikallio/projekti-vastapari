}
function doHideShow (doload, t, evt, pended) {
var r=true, s, i=true, once=false;
if (pended && (hX_6.pendingBlurRefocus!=null || hX_6.pendingFocusReturn!=null))
r=false;
else {
s=(t.id!=undefined)?hX_6.getComponentById(t.id):null;
if (s && s.onPreLoad!=undefined) i=s.onPreLoad(s.DOMobj, evt);
if (!i)
r=false;
else {
if (doload==1)
t.style.display=hX_6._E;
else if (doload==2)
t.style.display=hX_6._NO;
else if (doload==3)
hX_6.applyNestedProperty(t, hX_6._VIS, hX_6._VS);
else
 hX_6.applyNestedProperty(t, hX_6._VIS, hX_6._HI);
var e, tp, p, a=hX_6.getComponentsInId(t);
for (i=0; a!=null && i<a.length; i++) {
s=hX_6.getComponentById(a[i]);
if (s) {
if (s.redraw) s.redraw();
if (!once) {
if ((doload==1 || doload==3) && s.onLoad!=undefined) s.onLoad(s.DOMobj, evt);
if ((doload==2 || doload==4) && s.unLoad!=undefined) s.unLoad(s.DOMobj, evt);
once=true;
}
}
}
if (a!=null) delete a;
if (hX_6.isDef(typeof(ODCRegistry)) && ODCRegistry!=null && ODCRegistry.Elements) {
for (var e in ODCRegistry.Elements) {
if (ODCRegistry.Elements[e] && ODCRegistry.Elements[e].JSObject && ODCRegistry.Elements[e].JSObject.type && ODCRegistry.Elements[e].JSObject.type.toLowerCase()=="tabpanel") {
tp=ODCRegistry.Elements[e].JSObject;
p=(tp.getDOMobj)?tp.getDOMobj():null;
while (p!=null && p!=t) p=p.parentNode;
if (p==t && tp.redraw)tp.redraw();
}
}
}
}
}
return r;
}
function doClear (obj) {
if (obj && obj.tagName && obj.value && obj.value.length > 0 && (obj.tagName == hX_6._TXT || (obj.tagName == hX_6._INP && (obj.type.toLowerCase() == hX_6._XT ||obj.type.toLowerCase()==hX_6._PWD)))) {
var p=hX_6.sel.getCaretPos(obj);
if (p>=0 && p<obj.value.length) {
if (hX_6.brw.isIE()) hX_6.skipVCMonitor=true;
obj.value=obj.value.substr(0,p);
}
}
}
function dummyEvent (typ, tgt, evt, noclear) {
not=(noclear!=null && noclear==true);
this.dummyEvent=true;
this.type=typ;
this.target=tgt;
if (evt.errorMsg) this.errorMsg=evt.errorMsg;
if (evt.pageX!=undefined) {
this.pageX=evt.pageX;
this.pageY=evt.pageY;
}
if (evt.clientX!=undefined) {
this.clientX=evt.clientX;
this.clientY=evt.clientY;
}
if (evt.screenX!=undefined) {
this.screenX=evt.screenX;
this.screenY=evt.screenY;
}
if (evt.altKey!=undefined) this.altKey=evt.altKey;
if (evt.shiftKey!=undefined) this.shiftKey=evt.shiftKey;
if (evt.button!=undefined) this.button=evt.button;
if (evt.keyCode!=undefined) this.keyCode=evt.keyCode;
if (evt.fromElement!=undefined) {
if (evt.fromElement.id) this.fromElementId=evt.fromElement.id;
if (!not) this.fromElement=null;
}
if (evt.srcElement!=undefined) {
if (evt.srcElement.id) this.srcElementId=evt.srcElement.id;
if (!not) this.srcElement=null;
}
if (evt.toElement!=undefined) {
if (evt.toElement.id) this.toElementId=evt.toElement.id;
if (!not) this.fromElement=null;
}
if (evt.target!=undefined) {
if (evt.target.id) this.targetId=evt.target.id;
if (!not) this.target=null;
}
if (evt.currentTarget!=undefined) {
if (evt.currentTarget.id) this.currentTargetId=evt.currentTarget.id;
if (!not) this.currentTarget=null;
}
if (evt.originalTarget!=undefined) {
if (evt.originalTarget.id) this.originalTargetId=evt.originalTarget.id;
if (!not) this.originalTarget=null;
}
if (evt.relatedTarget!=undefined) {
if (evt.relatedTarget.id) this.relatedTargetId=evt.relatedTarget.id;
if (!not) this.relatedTarget=null;
}
}
function rebuildDummyEvent (evt) {
if (evt.fromElementId!=undefined) {
evt.fromElement=hX_6.getElementById(evt.fromElementId);
}
if (evt.srcElementId!=undefined) {
evt.srcElement=hX_6.getElementById(evt.srcElementId);
}
if (evt.toElementId!=undefined) {
evt.toElement=hX_6.getElementById(evt.toElementId);
}
if (evt.targetId!=undefined) {
evt.target=hX_6.getElementById(evt.targetId);
}
if (evt.currentTargetId!=undefined) {
evt.currentTarget=hX_6.getElementById(evt.currentTargetId);
}
if (evt.originalTargetId!=undefined) {
evt.originalTarget=hX_6.getElementById(evt.originalTargetId);
}
if (evt.relatedTargetId!=undefined) {
evt.relatedTarget=hX_6.getElementById(evt.relatedTargetId);
}
}
function doAction (srcobj, action, globalAction, target, evt, hasSubmit, hasStop) {
var r=true, doload=0, redraw=false, redrawHS=false, a, b, s=null, i, t=target, domMaxLen, a=(action)?action.toUpperCase():null;
if (srcobj != null && t != null && a != null) {
hX_6.skipVCMonitor=false;
switch (a) {
case hX_6.ACT_NOTHING:
case hX_6.ACT_NONE:
case hX_6.ACT_STOP:
r=false;
break;
case hX_6.ACT_WHILEEVENT:
break;
case hX_6.ACT_GOTO:
if (t) window.location.href=t;
break;
case hX_6.ACT_ALERT:
if (t && t.toUpperCase()==hX_6.TGT_ATERROR) t=(evt.errorMsg)?evt.errorMsg:hX_6.TGT_ISERROR;
if (t) alert (t);
break;
case hX_6.ACT_CONFIRM:
if (t && t.toUpperCase()==hX_6.TGT_ATERROR) t=(evt.errorMsg)?evt.errorMsg:hX_6.TGT_ISERROR;
if (t) {
s=confirm(t);
r=(s)?true:false;
}
break;
case hX_6.ACT_LOCKED:
hX_6.applyNestedAttribute(t, hX_6._DSB, true, true);
redraw=true;
break;
case hX_6.ACT_UNLOCKED:
hX_6.applyNestedAttribute(t, hX_6._DSB, false, true);
redraw=true;
break;
case hX_6.ACT_INVERTLOCKED:
hX_6.applyNestedAttribute(t, hX_6._DSB, ((t.disabled)?false:true), true);
redraw=true;
break;
case hX_6.ACT_SHOW:
redrawHS=true;
doload=1;
break;
case hX_6.ACT_HIDE:
redrawHS=true;
doload=2;
break;
case hX_6.ACT_INVERTHIDE:
redrawHS=true;
doload=(hX_6.g.getP(t,hX_6._DSP)==hX_6._NO)?1:2;
break;
case hX_6.ACT_VISIBLE:
redrawHS=true;
doload=3;
break;
case hX_6.ACT_INVISIBLE:
redrawHS=true;
doload=4;
break;
case hX_6.ACT_INVERTVISIBLE:
redrawHS=true;
doload=(hX_6.g.getP(t,hX_6._VIS)==hX_6._HI)?3:4;
break;
case hX_6.ACT_NEXTTAB:
hX_6.gotoNextPrev (t, true);
break;
case hX_6.ACT_NEXTTABCLEAR:
if (evt) {
s=hX_6.evtGetTarget(evt);
doClear(s);
}
hX_6.gotoNextPrev (t, true);
break;
case hX_6.ACT_PREVTAB:
hX_6.gotoNextPrev (t, false);
break;
case hX_6.ACT_PREVTABCLEAR:
if (evt) {
s=hX_6.evtGetTarget(evt);
doClear(s);
}
hX_6.gotoNextPrev (t, false);
break;
case hX_6.ACT_CLICK:
doClick(t);
r=false;
break;
case hX_6.ACT_GET:
while (s==null && t!=null) {
b=hX_6.getBehaviorsById(t);
if (b && b.length > 0) for (i=0; i<b.length; i++) {
if (b[i].type==hX_6.BHR_ajaxgetexternal || b[i].type==hX_6.BHR_ajaxget || b[i].type==hX_6.BHR_ajaxsubmit) {
s=b[i];
break;
}
}
t=t.parentNode;
}
if (s && s.dispatchBehavior) s.dispatchBehavior(null, evt);
break;
case hX_6.ACT_SUBMIT:
var tagset=[];
tagset.push(new hX_6.tagListItem(hX_6._INP,hX_6._SBM));
tagset.push(new hX_6.tagListItem(hX_6._INP,hX_6._lIMG));
tagset.push(new hX_6.tagListItem(hX_6._BTN,hX_6._SBM));
if (hX_6.isTag(t, tagset))
doClick(t);
else {
var submit, form=(t.tagName==hX_6._FRM) ? t : t.form;
if (form) {
submit=hX_6.findFirstTag(form, tagset);
if (submit) doClick(submit);
}
}
return false;
break;
case hX_6.ACT_PADLEFT:
if (t && t.value && t.maxLength) {
domMaxLen=(t.maxLength == -1 || t.maxLength == 0x7fffffff || t.maxLength == void(0)) ? 0 : t.maxLength;
if (domMaxLen > 0) {
if (hX_6.brw.isIE()) hX_6.skipVCMonitor=true;
t.value=hX_6.padL(t.value, domMaxLen, hX_6._B);
}
}
break;
case hX_6.ACT_PADRIGHT:
if (t && t.value && t.maxLength) {
domMaxLen=(t.maxLength == -1 || t.maxLength == 0x7fffffff || t.maxLength == void(0)) ? 0 : t.maxLength;
if (domMaxLen > 0) {
if (hX_6.brw.isIE()) hX_6.skipVCMonitor=true;
t.value=hX_6.padR(t.value, domMaxLen, hX_6._B);
}
}
break;
case hX_6.ACT_TRIM:
if (t && t.value) {
if (hX_6.brw.isIE()) hX_6.skipVCMonitor=true;
t.value=hX_6.Trim(t.value);
}
break;
case hX_6.ACT_MOVE:
if (evt && evt.keyCode!=undefined && t) {
if (evt.keyCode==hX_6.KEY_LTARROW || evt.keyCode==hX_6.KEY_RTARROW) {
s=hX_6.g.getAbsPosLeft(t)+((evt.keyCode==hX_6.KEY_LTARROW)?-5:5);
i=hX_6.g.getWindowClientWidth()-hX_6.g.getRendWidth(t);
s=(s<0)?0:((s>i)?i:s);
if (hX_6.g.getRelPos(t)) s=s - hX_6.g.getAbsPosLeft(hX_6.g.getRelPos(t));
t.style.left=s+hX_6._PX;
} else if (evt.keyCode==hX_6.KEY_UPARROW || evt.keyCode==hX_6.KEY_DNARROW) {
s=hX_6.g.getAbsPosTop(t)+((evt.keyCode==hX_6.KEY_UPARROW)?-5:5);
i=hX_6.g.getWindowClientHeight()-hX_6.g.getRendHeight(t);
s=(s<0)?0:((s>i)?i:s);
if (hX_6.g.getRelPos(t)) s=s - hX_6.g.getAbsPosTop(hX_6.g.getRelPos(t));
t.style.top=s+hX_6._PX;
}
}
break;
case hX_6.ACT_DLG_OK:
redrawHS=true;
doload=2;
evt.submitFlag=hX_6.ACT_DLG_OK;
break;
case hX_6.ACT_DLG_CANCEL:
redrawHS=true;
doload=2;
evt.submitFlag=hX_6.ACT_DLG_CANCEL;
break;
case hX_6.ACT_FOCUS:
case hX_6.ACT_SELECTED:
case hX_6.ACT_UNSELECTED:
r=false;
if (evt && evt.type==hX_6._evF && t.id==srcobj.id) {
if (a == hX_6.ACT_SELECTED)
hX_6.sel.setCaretPos(srcobj, 0, srcobj.value.length, hX_6.brw.isNavOrMoz());
else {
var bev=hX_6.getBehaviorById(srcobj.id, hX_6.BHR_assist, hX_6._onIN);
if ((a==hX_6.ACT_UNSELECTED) || (bev==null) || (bev!=null && bev.assist==null))
hX_6.sel.setCaretPos(srcobj, 0, 0, hX_6.brw.isNavOrMoz());
}
break;
}
hX_6.i.setFocus(t, (a==hX_6.ACT_SELECTED), true);
if (evt && evt.type==hX_6._evB) hX_6.pendingBlurRefocus=t.id;
if (!globalAction) {
hX_6.pendingFocusReturn=t.id;
hX_6.pendingSelect=(a==hX_6.ACT_SELECTED);
}
break;
default:
break;
}
}
if (redrawHS) {
var se,tid,isDlg=false;
s=(t.id!=undefined)?hX_6.getComponentById(t.id):null;
if (s && s.type==hX_6.CMP_dialog && (doload==2 || doload==4) && hasSubmit!=null && hasStop!=null) {
isDlg=hasSubmit;
isDlg=(isDlg)?isDlg:((!hasStop && srcobj.tagName && srcobj.tagName==hX_6._INP && (srcobj.type.toLowerCase()==hX_6._SBM || srcobj.type.toLowerCase()==hX_6._RES || srcobj.type.toLowerCase()==hX_6._lIMG)));
isDlg=(isDlg)?isDlg:(srcobj.tagName && srcobj.tagName==hX_6._INP && srcobj.type.toLowerCase()==hX_6._RES);
if (isDlg) {
hX_6.pendingBlurRefocus=null;
hX_6.pendingFocusReturn=null;
}
}
if (s && globalAction && !isDlg && (s.onLoad || s.onPreLoad)) {
if (document.createEventObject!=undefined)
se=hX_6.i.duplicateEvent(evt);
else
 se=new dummyEvent(evt.type, evt.target, evt, true);
if (evt.submitFlag) se.submitFlag=evt.submitFlag;
tid=hX_6.i.startDspTimerTimed("hX_6.action.doHideShowCB", 1, null, true);
hX_6.action.sBlocks.push([tid,doload,t,se]);
r=true;
} else {
r=doHideShow (doload, t, evt, false);
}
}
if (redraw) {
a=hX_6.getComponentsInId(t);
for (s=0; a!=null && s<a.length; s++) {
s=hX_6.getComponentById(a[s]);
if (s) s.redraw();
}
}
if (!r) hX_6.i.cancelAction(evt, true);
return r;
}
function doClick (inObj, evttag) {
var o=hX_6.getElementById(inObj);
if (o.click && hX_6.brw.isIE())
return (o.click());
else {
var r=hX_6.i.fireEvent (o, hX_6._onMC, evttag);
if (r == null || r == true && (!hX_6.brw.isIE() && o.tagName && o.tagName==hX_6._ANC)) {
if (o.href) window.location.href=o.href;
}
return r;
}
}
function processAction (obj, evt, fcn, action, globalAction, target, cssStyle, cancelBubble, retainFocus, isError) {
return this.processActionEx (obj, evt, fcn, action, globalAction, target, cssStyle, cancelBubble, retainFocus, isError,
null, null, null, false, null, false, null, false, null);
}
function processActionEx (obj, evt, fcn, action, globalAction, target, cssStyle, cancelBubble, retainFocus, isError,
error, label, cssLabelStyle, popLabels, infoSpan, infoShow, errorSpan, errorShow, reEntry) {
var skipPreventDefault=(evt && evt.getPreventDefault && evt.getPreventDefault())?true:false;
if (reEntry==null) {
this.pRunningCount++;
if (this.pRunningCount > 10000) this.pRunningCount=1;
}
var id=this.pRunningCount;
var r=(reEntry!=null)?true:this.processFunction(obj,evt,fcn);
var k, u, t, a, initialr=r, pause=null, hasSubmit=false, hasStop=false, whileFound=null, whileTime=null, whileName=null;
if (r && action != null) {
for (k=((reEntry!=null)?reEntry:0); r && pause==null && k < action.length; k++) {
u=action[k].toUpperCase();
hasSubmit=(u==hX_6.ACT_SUBMIT || u==hX_6.ACT_CLICK || u==hX_6.ACT_DLG_OK || u==hX_6.ACT_DLG_CANCEL);
hasStop=(u==hX_6.ACT_NOTHING || u==hX_6.ACT_NONE || u==hX_6.ACT_STOP);
whileFound=(reEntry==null && u==hX_6.ACT_WHILEEVENT)?k:whileFound;
}
if (whileFound!=null) {
whileTime=(target[whileFound]!=null)?parseInt(target[whileFound],10):20;
if (evt!=null) whileName=(evt.type==hX_6._evMV)?hX_6._onMO:((evt.type==hX_6._evF)?hX_6._onB:null);
if (whileName==hX_6._onMO)
this.whileMOTrapOn=hX_6.attachEvent(obj, whileName, hX_6._lF, hX_6.action.whileMOTrapper);
if (whileName==hX_6._onB)
this.whileBTrapOn=hX_6.attachEvent(obj, whileName, hX_6._lF, hX_6.action.whileBTrapper);
var pauseFid=hX_6.action.pauseActions(), pauseF=this.isPaused(pauseFid);
pauseF.defer (whileTime, obj, evt, fcn, action, globalAction, target, cssStyle, cancelBubble, retainFocus, isError, error, label, cssLabelStyle, popLabels, infoSpan, infoShow, errorSpan, errorShow, 0);
hX_6.action.unPauseActions(pauseFid);
return false;
}
for (k=((reEntry!=null)?reEntry:0); r && pause==null && k < action.length; k++) {
a=action[k].toUpperCase();
t=target[k];
if (!(a==hX_6.ACT_CONFIRM || a==hX_6.ACT_ALERT)) {
if (hX_6.isDef(typeof t) && t != null) {
if (t.toLowerCase() == hX_6._NO) t=document.body;
else if (t.substr(0,4).toLowerCase() == hX_6._URL || t.substr(0,4).toLowerCase() == hX_6._URI) t=hX_6.stripURL(t);
else t=hX_6.getElementById(t);
} else if (evt != null) t=hX_6.evtGetTarget(evt);
}
r=doAction (obj, a, globalAction, t, evt, hasSubmit, hasStop);
r=(hX_6.isUnDef(typeof r)) ? true : ((r == false) ? false: true);
if (evt && evt.returnValue!=undefined && evt.returnValue == false) r=false;
else if (evt && evt.getPreventDefault && !skipPreventDefault && evt.getPreventDefault()) r=false;
if (r && a==hX_6.ACT_GET) {
pause=this.isPaused(id);
if (pause!=null) pause.defer (20, obj, evt, fcn, action, globalAction, target, cssStyle, cancelBubble, retainFocus, isError, error, label, cssLabelStyle, popLabels, infoSpan, infoShow, errorSpan, errorShow, k+1);
var isSbm, tagset=[];
tagset.push(new hX_6.tagListItem(hX_6._INP,hX_6._SBM));
tagset.push(new hX_6.tagListItem(hX_6._INP,hX_6._RES));
tagset.push(new hX_6.tagListItem(hX_6._INP,hX_6._lIMG));
tagset.push(new hX_6.tagListItem(hX_6._BTN,hX_6._SBM));
isSbm=hX_6.isTag(obj, tagset);
delete tagset;
if (isSbm || hasStop) r=false;
}
}
}
if (!pause) {
if (this.whileBTrapOn!=null || hX_6.action.whileMOTrapOn!=null) {
if (hX_6.action.whileMOTrapOn!=null) hX_6.removeEvent(document.body, hX_6._onMO, hX_6.action.whileMOTrapOn);
if (hX_6.action.whileBTrapOn!=null) hX_6.removeEvent(document.body, hX_6._onB, hX_6.action.whileBTrapOn);
hX_6.action.whileBTrapOn=hX_6.action.whileMOTrapOn=null;
}
if (initialr) {
doCssStyleClass (obj, cssStyle, label, cssLabelStyle);
if (popLabels!=null) this.positionLabels(obj, popLabels, infoSpan, errorSpan);
doHideShowLabels (popLabels, infoSpan, infoShow, errorSpan, errorShow, error);
if (popLabels!=null) this.positionLabels(obj, popLabels, infoSpan, errorSpan);
}
if (cancelBubble != null || (isError && !r)) {
if (evt) {
if (evt.preventDefault) {
if (evt.stopPropagation) evt.stopPropagation();
if (evt.preventDefault) evt.preventDefault();
} else {
if (hX_6.brw.isIE() && evt.type==hX_6._evKD) {
var code=(evt.keyCode && (evt.keyCode != 0)) ? evt.keyCode : -1;
if ((code >= 91 && code <= 93) || (code >= 112 && code <= 123))
try {
evt.keyCode=0;
} catch (e) {
}
}
evt.cancelBubble=true;
evt.returnValue=false;
}
}
r=false;
}
if (retainFocus) hX_6.i.setFocus(t, false, true);
}
return r;
}
function processFunction (obj, evt, fcn, arg0) {
var r=true;
if (fcn != null && obj!=null) {
try {
var realfcn=(hX_6.isFunction(fcn)) ? fcn : new Function ("event", fcn);
r=(arg0==null) ? realfcn.call(obj, evt) : realfcn.call(obj, arg0, evt);
r=(hX_6.isUnDef(typeof r)) ? true : ((r == false) ? false: true);
if (evt) {
if (evt.returnValue!=undefined && evt.returnValue == false) r=false;
else if (evt.getPreventDefault && evt.getPreventDefault()) r=false;
}
}
catch (e) {
window.status=hX_6.INTERR_Dispatcher;
if (evt && evt.errorMsg) evt.errorMsg=null;
}
}
return r;
}
}
HxG_6.prototype.HxCaretSelection=function () {
}
HxG_6.prototype.HxCaretSelection.prototype.getSelection=function (obj) {
var r, sel=null;
try {
if (obj.selectionEnd) {
sel=obj.value.substring(obj.selectionStart,obj.selectionEnd);
} else if (document.selection) {
r=document.selection.createRange();
sel=r.text;
}
} catch (ex) { sel=null; }
return sel;
}
HxG_6.prototype.HxCaretSelection.prototype.getSelectionR=function (obj) {
var s=0, e=0, l;
try {
if (obj.selectionEnd) {
s=obj.selectionStart;
e=obj.selectionEnd;
} else if (document.selection) {
s=this.getCaretPos(obj);
l=this.getSelection(obj);
e=s+l.length;
}
s=(s < 0) ? 0 : s;
e=(e > obj.value.length) ? obj.value.length : e;
} catch (ex) { s=0;e=0; }
return{start: s, end: e};
}
HxG_6.prototype.HxCaretSelection.prototype.hasSelection=function (obj) {
var sel=hX_6.sel.getSelection(obj);
return (sel != null && sel.length > 0);
}
HxG_6.prototype.HxCaretSelection.prototype.setSelection=function (obj, start, end) {
if (obj.createTextRange) {
var t=((obj.tagName) ? obj.tagName : hX_6._E), y=((obj.type) ? obj.type.toLowerCase() : hX_6._E);
if (t==hX_6._BBODY || t==hX_6.TXT || t==hX_6.__BTN ||
(t==hX_6._INP && (y==hX_6._TBTN || y==hX_6._HI || y==hX_6._PWD || y==hX_6._RES || y==hX_6._SBM || y==hX_6._XT)))
{
var r=obj.createTextRange();
r.collapse(true);
r.moveEnd('character', end);
r.moveStart('character', start);
r.select();
}
} else if (obj.setSelectionRange) {
try {
obj.setSelectionRange(start, (end==null)?start:end);
} catch (e) {
}
}
}
HxG_6.prototype.HxCaretSelection.prototype.getCaretPos=function (obj, hasFocus) {
if (hasFocus != null && !hasFocus)
if (obj.focus) obj.focus();
if (obj.selectionStart) {
return obj.selectionStart;
} else if (document.selection) {
var n, r=document.selection.createRange();
r.moveEnd("textedit");
if (obj != r.parentElement())
n=-1;
else
 n=obj.value.length - r.text.length;
return n;
}
return 0;
}
HxG_6.prototype.HxCaretSelection.prototype.setCaretPos=function (obj, pos, pos1, defer) {
if (arguments.length > 3 && defer != null && defer) {
hX_6.i.deferSetFocus=obj;
hX_6.i.deferSetSelect=pos;
hX_6.i.deferSetSelect1=(arguments.length > 2 && pos1 != null) ? pos1 : null;
hX_6.i.startDspTimerTimed("hX_6.i.selSetter", 1);
} else
 hX_6.sel.setSelection(obj, pos, pos1);
}
HxG_6.prototype.HxCompImpMgr.prototype.selSetter=function(unq) {
if ((typeof hX_6)=="undefined") return;
hX_6.i.qKillTimer(unq);
hX_6.sel.setSelection(hX_6.i.deferSetFocus, hX_6.i.deferSetSelect, ((hX_6.i.deferSetSelect1 != null)?hX_6.i.deferSetSelect1:hX_6.i.deferSetSelect));
}
HxG_6.prototype.HxRect=function (obj) {
this.X=this.Y=this.width=this.height=null;
if (arguments.length > 0) this.setRect(obj);
}
HxG_6.prototype.HxRect.prototype.setRect=function (obj) {
if (obj) {
this.X=hX_6.g.getAbsPosLeft(obj);
this.Y=hX_6.g.getAbsPosTop(obj);
this.width=hX_6.g.getRendWidth(obj);
this.height=hX_6.g.getRendHeight(obj);
}
}
HxG_6.prototype.HxRect.prototype.setRectR=function (r) {
if (r) {
this.X=r.X;
this.Y=r.Y;
this.width=r.width;
this.height=r.height;
}
}
HxG_6.prototype.HxRect.prototype.incRect=function (i) {
this.X+=i;
this.Y+=i;
this.width+=(i*2);
this.height+=(i*2);
}
HxG_6.prototype.HxRect.prototype.hitTest=function (iX, iY) {
if (this.X == null || this.Y == null || this.width == null || this.height == null)
return false;
var x, y;
if (arguments.length == 1) {
x=iX.X; y=iX.Y;
} else {
x=iX; y=iY;
}
return ((x >= this.X) && (x < (this.X+this.width)) &&
(y >= this.Y) && (y < (this.Y+this.height)));
}
HxG_6.prototype.HxGeometryMgr.prototype.toPoint=function (evt) {
var cX, cY;
if (hX_6.brw.isIE()) {
var sBase=(hX_6.brw.isIE6_CSS()) ? document.body.parentNode : document.body;
cX=evt.clientX; cY=evt.clientY;
cX=cX + sBase.scrollLeft;
cY=cY + sBase.scrollTop;
} else {
cX=evt.pageX;	cY=evt.pageY;
}
return {X: cX, Y: cY};
}
HxG_6.prototype.pseudoClipboard=function () {
this.valueAsString=null;
this.valueAsNumber=null;
}
HxG_6.prototype.HxIFrameMgr=function (thisparent) {
this.p=thisparent;
}
HxG_6.prototype.HxIFrameMgr.prototype.create=function (doc, id, invisible, transparent) {
var o=null, d;
if (id && doc.createElement) {
o=doc.createElement(hX_6._IFRM);
o.setAttribute (hX_6._ID, id);
o.setAttribute (hX_6._NAM, id);
o.src="javascript:'';";
if (invisible) {
o.style.position=hX_6._ABS;
o.style.top=hX_6._ZPX;
o.style.left=hX_6._ZPX;
o.style.width=hX_6._1PX;
o.style.height=hX_6._1PX;
o.style.borderWidth=hX_6._ZPX;
o.style.borderStyle=hX_6._NO;
o.style.display=hX_6._NO;
} else {
