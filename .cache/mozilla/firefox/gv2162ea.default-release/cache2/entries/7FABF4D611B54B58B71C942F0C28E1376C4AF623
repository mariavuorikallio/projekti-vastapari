this.m_isIE=false;
this.m_isNavigator=true;
this.m_browserLevel=4;
this.m_realAppVersion=4.7;
}
this.m_hasIECacheBug=this.m_isIE;
this.m_hasStyleSheet=this.isBrowserLevel(4);
