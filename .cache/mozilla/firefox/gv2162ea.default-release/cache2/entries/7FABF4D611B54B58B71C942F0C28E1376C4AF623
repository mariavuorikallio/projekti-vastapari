intD=hX_6.cal.daysInMonth(intM-1,intYT);
fixD=r=true;
} else if (intYL != intY && intD <= hX_6.cal.daysInMonth(intM-1,intYL)) {
intY=intYL;
fixY=r=true;
} else {
