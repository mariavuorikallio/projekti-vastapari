this.DF_MaxLength=this.DF_MaxPadLength=null;
this.isExponent=this.isExponentSigned=this.isPercent=this.isMille=false;
this.FMTChars=";0#.,E";
this.PREChars=hX_6.STR_percent_char+hX_6.STR_mille_char+hX_6.STR_icurrency_char;
this.hardPlus="+";
this.hardMinus="-";
this.hardE="E";
this.hardS="*";
this.xDigits=this.zeroDigit=null;
this.LC_groupingSymbol=this.LC_decimalSymbol=this.LC_percentSymbol=this.LC_milleSymbol=this.LC_minusSymbol=this.LC_currencySymbol=null;
this.LC_paddingChar=hX_6._Z;
this.msgFail=this.intMsgFail=hX_6._E;
this.signedConverter=this.zeroConverter=null;
this.attributeTable=[];
this.attributeTable ['strict']=['nStrict',		hX_6.ARG_TO_NUM,	false,	0,	2];
this.attributeTable ['pattern']=['pattern',		hX_6.ARG_TO_NAS,	false,	null,	null];
this.attributeTable ['locale']=['locale',		hX_6.ARG_TO_NAS,	false,	null,	null];
