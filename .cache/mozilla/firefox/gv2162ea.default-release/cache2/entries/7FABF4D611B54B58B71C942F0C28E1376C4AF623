["-1000", "HST", "-0900", "HDT",  "Hawaii-Aleutian Standard Time"],
["","","",""]];
hX_6.ERR_datetime_parse_fail= "\"{0}\" cannot be interpreted as a date or time.";
hX_6.ERR_epoch_name="\"{0}\" cannot be interpreted as a date. The epoch is not correct.";
hX_6.ERR_day_name="\"{0}\" cannot be interpreted as a date. The day name is not correct.";
hX_6.ERR_month_name="\"{0}\" cannot be interpreted as a date. The month name is not correct.";
