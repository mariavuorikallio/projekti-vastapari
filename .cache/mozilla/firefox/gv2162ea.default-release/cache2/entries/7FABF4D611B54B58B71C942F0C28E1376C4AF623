HxG_6.prototype.HxGeometryMgr.prototype.CSStagToDOMtag=function(value) {
var w=value, q=w.indexOf(hX_6._Y);
while (q >= 0) {
w=w.substring(0, q) + (w.substring(q+1, q+2)).toUpperCase() + w.substring(q+2);
q=w.indexOf(hX_6._Y);
}
return w;
}
HxG_6.prototype.HxGeometryMgr.prototype.findContainer=function (obj, p0, p1, p2, p3, p4, p5, p6, p7, p8, p9) {
