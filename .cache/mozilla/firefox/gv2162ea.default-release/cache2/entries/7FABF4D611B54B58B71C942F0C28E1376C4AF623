i+=cvt.abbEpochLength;
} else if (m != hX_6._lY) {
j=(m==hX_6._uA)?intMn:((m==hX_6._lS)?intS:((m==hX_6._lH)?intH:((m==hX_6._lM)?intM:((m==hX_6._lD)?intD:-1))));
if (j >= 0) {
