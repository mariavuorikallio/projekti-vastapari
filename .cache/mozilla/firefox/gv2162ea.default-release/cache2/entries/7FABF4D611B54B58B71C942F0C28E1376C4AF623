strE=cvt.descLbEpoch[j][1];
break;
}
}
}
if (strE != hX_6._E) fixG=r=true;
}
i+=cvt.abbEpochLength;
} else if (m != hX_6._lY) {
c1=cvt.getDigit(c1);
c2=((i+1)<l) ? cvt.getDigit(test.charAt(i+1)) : hX_6._B;
if (c1 == pChar && c2 == pChar) {
c1=hX_6._Z;
c2=(m==hX_6._lH || m==hX_6._lS || m==hX_6._uA) ? hX_6._Z:hX_6._1;
} else if (c1 == pChar && c2 != pChar) {
c1=hX_6._Z;
} else if (c1 != pChar && c2 == pChar) {
c2=(c1==hX_6._Z) ? hX_6._1 : c1;
c1=hX_6._Z;
} else if (c1 == hX_6._Z && c2 == hX_6._Z && m != hX_6._lH) {
c1=hX_6._Z;
c2=(m==hX_6._lH || m==hX_6._lS || m==hX_6._uA) ? hX_6._Z:hX_6._1;
}
if (!cvt.matchDigit(c1) || !cvt.matchDigit(c2))
return {reset:true, value:null};
j=parseInt(c1+c2,10);
if (m==hX_6._lH) intH=j;
else if (m==hX_6._uA) intMn=j;
else if (m==hX_6._lS) intS=j;
else if (m==hX_6._lM) intM=j;
else intD=j;
i=i+1;
} else {
j=(cvt.isShortYr)?2:4;
c2=((i+1)<l) ? test.charAt(i+1) : hX_6._B;
c3=((i+2)<l) ? test.charAt(i+2) : hX_6._B;
c4=((i+3)<l) ? test.charAt(i+3) : hX_6._B;
