}
pop.insertBefore (ds, pop.firstChild);
}
if (this.transparency && this.transDiv) {
ds=this.transDiv;
var lB=hX_6.g.parseBorder(pop, hX_6._LFT,0), rB=hX_6.g.parseBorder(pop, hX_6._RGT,0);
var tB=hX_6.g.parseBorder(pop, hX_6._TOP,0),  bB=hX_6.g.parseBorder(pop, hX_6._BOT,0);
