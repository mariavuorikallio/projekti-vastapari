}
function positionLabels (inObj, pop, spInfo, spError) {
if (inObj && (spInfo || spError)) {
var o=inObj;
if (o.id && hX_6.getComponentById(o.id)!=null) {
var cp=hX_6.getComponentById(o.id);
if (cp && cp.JSFBTN && o.parentNode.id && o.parentNode.id.indexOf("_outerspan">0)) {
o=o.parentNode;
}
}
var d, p,l,t,h, rel=false, top=hX_6.g.getAbsPosTop(o), left=hX_6.g.getAbsPosLeft(o), sTop=top, sLeft=left;
if (pop=="RIGHT") left+=hX_6.g.getRendWidth(o);
else if (pop=="UNDER") top+=hX_6.g.getRendHeight(o);
else rel=true;
sLeft=left; sTop=top;
if (spError) {
l=hX_6.g.getAbsPosLeft(spError); t=hX_6.g.getAbsPosTop(spError);
if (rel) {
d=hX_6.g.getP(spError, hX_6._DSP);
if (d==hX_6._NO) hX_6.g.setP(spError, hX_6._DSP, hX_6._BLK);
if (pop=="LEFT") left -= hX_6.g.getRendWidth(spError);
else top -= hX_6.g.getRendHeight(spError);
if (d!=hX_6.g.getP(spError, hX_6._DSP)) hX_6.g.setP(spError, hX_6._DSP, d);
}
if (l!= left) hX_6.g.setP(spError, hX_6._LFT, left+hX_6._PX);
if (t!= top) hX_6.g.setP(spError, hX_6._TOP, top+hX_6._PX);
}
if (spInfo) {
l=hX_6.g.getAbsPosLeft(spInfo); t=hX_6.g.getAbsPosTop(spInfo);
if (rel) {
d=hX_6.g.getP(spInfo, hX_6._DSP);
if (d==hX_6._NO) hX_6.g.setP(spInfo, hX_6._DSP, hX_6._BLK);
if (pop=="LEFT") sLeft -= hX_6.g.getRendWidth(spInfo);
else sTop -= hX_6.g.getRendHeight(spInfo);
if (d!=hX_6.g.getP(spInfo, hX_6._DSP)) hX_6.g.setP(spInfo, hX_6._DSP, d);
}
if (l!= sLeft) hX_6.g.setP(spInfo, hX_6._LFT, sLeft+hX_6._PX);
if (t!= sTop) hX_6.g.setP(spInfo, hX_6._TOP, sTop+hX_6._PX);
}
}
}
function doHideShowCB (unq) {
if ((typeof hX_6)=="undefined") return true;
