obj=hX_6.getComponentById(inputfld.id);
if (obj) {
obj.redraw(true);
return false;
}
}
}
}
}
return true;
}
HxG_6.prototype.JSFSubButton.prototype.dispatchMonitorBtn=function(obj, evt, args) {
