return (null);
}
HxG_6.prototype.MaskConverter.prototype.valueToConstant=function (s) {
return (null);
}
HxG_6.prototype.StringValidator=function () {
this.type=hX_6.CVT_string;
this.lc=hX_6.currentLocale();
this.minimumLength=this.maximumLength=this.required=null;
this.constraint=this.constraintImp=null;
this.constraintExp=this.constraintExpImp =null;
this.ELexpression=this.ELexpressionImp=null;
this.msgFail=hX_6._E;
this.attributeTable=[];
this.attributeTable ['required']=['required',		hX_6.ARG_IS_TRUE,	false,	null, null];
this.attributeTable	['minimum-length']=['minimumLength',	hX_6.ARG_TO_NUM,	false,	0, 9999999];
this.attributeTable	['min-length']=['minimumLength',	hX_6.ARG_TO_NUM,	false,	0, 9999999];
this.attributeTable	['maximum-length']=['maximumLength',	hX_6.ARG_TO_NUM,	false,	1, 9999999];
this.attributeTable	['max-length']=['maximumLength',	hX_6.ARG_TO_NUM,	false,	1, 9999999];
this.attributeTable	['constraint']=['constraint',	hX_6.ARG_TO_NAN,	false,	null, null];
this.attributeTable	['constraint-regexpression']= ['constraintExp',	hX_6.ARG_TO_NAN,	false,	null, null];
this.attributeTable	['constraint-expression']=['ELexpression',	hX_6.ARG_TO_NAN,	false,	null, null];
this.setAttribute(arguments);
}
HxG_6.prototype.StringValidator.prototype.setAttribute=function(a) {
hX_6.parseArg (this.attributeTable, this, a);
if (this.minimumLength != null && this.minimumLength < 0) this.minimumLength=0;
if (this.maximumLength != null && this.maximumLength < 1) this.maximumLength=1;
if (this.maximumLength && this.minimumLength && this.minimumLength >= this.maximumLength) this.minimumLength=this.maximumLength=null;
if (this.constraint) this.constraintImp=hX_6.regExp.getRegExpByName(this.constraint);
if (this.constraintExp) this.constraintExpImp=hX_6.regExp.registerConstraint(this.constraintExp);
if (this.ELexpression != null) this.ELexpressionImp=hX_6.regExp.registerELConstraint(this.ELexpression);
return true;
}
HxG_6.prototype.StringValidator.prototype.getAttribute=function (attribute) {
return (hX_6.getArg(this.attributeTable, this, attribute));
}
HxG_6.prototype.StringValidator.prototype.lastError=function () {
return ((this.msgFail && this.msgFail.length > 0) ? this.msgFail : null);
}
HxG_6.prototype.StringValidator.prototype.validate=function (teststr, iC) {
this.msgFail=hX_6._E;
var C=(iC != null) ? hX_6.getConverterById(iC) : null;
if (teststr != null && !hX_6.isString(teststr)) {
this.msgFail=hX_6.L[this.lc].ERR_required;
return false;
}
var test=(teststr == null) ? hX_6._E : teststr;
test=test.replace(/[\r\n]/g, hX_6._E);
if (test.length == 0) {
if (this.required) {
this.msgFail=hX_6.L[this.lc].ERR_required;
return false;
} else
 return true;
}
if (this.minimumLength && test.length < this.minimumLength) {
this.msgFail=hX_6.L[this.lc].ERR_minimum;
return false;
}
if (this.maximumLength && test.length > this.maximumLength) {
this.msgFail=hX_6.L[this.lc].ERR_maximum;
return false;
}
if (this.constraintImp != null) {
if (!hX_6.regExp.testRegExp(test, this.constraintImp)) {
this.msgFail=hX_6.L[this.lc].ERR_constraint;
return false;
}
}
if (this.constraintExpImp != null) {
if (!hX_6.regExp.testRegExp(test, this.constraintExpImp)) {
this.msgFail=hX_6.L[this.lc].ERR_constraint;
return false;
}
}
if (this.ELexpressionImp != null) {
if (!hX_6.regExp.testELExp(test, this.ELexpressionImp)) {
this.msgFail=hX_6.L[this.lc].ERR_constraint;
return false;
}
}
return true;
}
HxG_6.prototype.MaskAssist=function (_elem, _state, _converter, _validator, _promptChar) {
if (!_elem || !_converter || !_state) return;
this.lc=hX_6.currentLocale();
this.isFilled=isFilled;
this.onFocus=onFocus;
this.onBlur=onBlur;
this.onClick=onClick;
this.onCut=onCut;
this.onPaste=onPaste;
this.onChangeKeyCode= onChangeKeyCode;
this.onChangeChrCode= onChangeChrCode;
this.onValidate=onValidate;
this.destroy=destroy;
var elem=_elem;
var state=_state;
var cvt=_converter;
var validator=_validator;
var promptChar=_promptChar;
this.getMask=function getMask() { return ((cvt.maskNFormat!=null)?cvt.maskNFormat:cvt.maskFormat); }
this.getMaskLast=function getMaskLast() { return ((cvt.maskNFormat!=null)?cvt.maskNFormatLast:cvt.maskFormatLast); }
this.getMaskFirst=function getMaskFirst() { return ((cvt.maskNFormat!=null)?cvt.maskNFormatFirst:cvt.maskFormatFirst); }
var handler=null;
handler=new hX_6.MaskAssist.prototype.maskHandler(elem, state, cvt, promptChar);
function destroy() {
if (handler!=null) {
if (handler.destroy) handler.destroy();
delete handler;
}
}
function isFilled (curpos) {
var c, v=hX_6.Trim(elem.value), l=v.length;
if (l < (this.getMask()).length)
return false;
c=v.charAt(this.getMaskLast());
return ((curpos > this.getMaskLast()) && (c != promptChar) && (c != hX_6._B));
}
function onFocus () {
var newValue=(elem.value) ? hX_6.trimR(elem.value) : null;
newValue=(newValue && newValue.length > 0) ? cvt.stringToValue(newValue, promptChar, true) : null;
newValue=(newValue) ? cvt.valueToString(newValue, promptChar) : handler.getEmptyMask();
elem.value=newValue;
handler.rebuildMask(newValue);
hX_6.sel.setCaretPos(elem, this.getMaskFirst(), null, hX_6.brw.isNavOrMoz());
return true;
}
function onBlur () {
var v=elem.value;
if (v.length==0) return true;
var newValue=cvt.stringToValue(v, promptChar);
if (newValue != null)
newValue=(newValue.length==0) ? hX_6._E : cvt.valueToString(newValue);
if (newValue != null) {
var m=handler.getEmptyMask();
m=hX_6.replaceAll(m,promptChar,hX_6._B);
if (m!=newValue)
elem.value=newValue;
else
 elem.value=hX_6._E;
return true;
}
return false;
}
function onClick (evt) {
return handler.onClick(evt);
}
function onCut (evt) {
return handler.onCut(evt);
}
function onPaste (evt) {
return handler.onPaste(evt);
}
function onChangeKeyCode (evt) {
return handler.onChangeKeyCode (evt);
}
function onChangeChrCode (evt) {
return handler.onChangeChrCode (evt);
}
function onValidate () {
var newValue=cvt.stringToValue(state.getValue(), promptChar);
if (newValue != null)
newValue=cvt.valueToString(newValue, promptChar);
if (newValue != null)
return true;
return false;
}
}
HxG_6.prototype.MaskAssist.prototype.maskHandler=function(_element, _state, _converter, _promptChar) {
var elem=_element;
var state=_state;
var cvt=_converter;
var promptChar=_promptChar;
var emptyMask=null;
this.onClick=onClick;
this.onCut=onCut;
this.onPaste=onPaste;
this.onChangeKeyCode= onChangeKeyCode;
this.onChangeChrCode= onChangeChrCode;
this.getEmptyMask=function getEmptyMask() {return emptyMask;}
this.rebuildMask=function rebuildMask(v) {if (cvt.rebuildProtectMask) cvt.rebuildProtectMask(v); }
var getKeyCode=hX_6.getKeyCode;
var getMask=function getMask() { return ((cvt.maskNFormat!=null)?cvt.maskNFormat:cvt.maskFormat); }
var getCChar=function (i) 	{ return (i < 0 || i >= (getMask()).length) ? hX_6._E : (getMask()).charAt(i); }
emptyMask=hX_6.replaceAll(cvt.maskFormat, hX_6._LB, promptChar);
for (var i=1; i<cvt.markers.length; i++)
emptyMask=hX_6.replaceAll(emptyMask, cvt.markers.charAt(i), promptChar);
function doDelete (evt) {
var v, s, cR, pos=state.getPos();
pos=(evt.type==hX_6._evCT) ? (pos) : ((evt.keyCode==hX_6.KEY_BS) ? pos-1 : pos);
if (pos < 0 && !state.hasSel())
return true;
cR=getCChar(pos);
if (!cvt.isMarker(cR) && cR != hX_6._EQ && !state.hasSel()) {
if (evt.type!=hX_6._evCT) {
pos=(evt.keyCode==hX_6.KEY_BS) ? (pos) : (pos+1);
state.setPos(pos);
return -2;
}
return false;
}
v=state.getValue(); s=state.hasSel();
if (s) {
var rg=hX_6.sel.getSelectionR(elem);
state.setValue((rg.start <= 0 && rg.end >= v.length-1)?emptyMask:fillWithMaskPiece(v, rg.start, rg.end));
} else
 if (pos>=0 && pos<v.length) state.setValue (v.substr(0,pos) + promptChar + v.substr(pos+1));
pos=(s || (evt.type!=hX_6._evCT && evt.keyCode==hX_6.KEY_BS)) ? pos : pos+1;
state.setPos(pos);
return -2;
}
function doInsert (v, chi, pos, sel) {
var ch=(chi==hX_6._B)?promptChar:chi;
if (sel) {
var rg=hX_6.sel.getSelectionR(elem);
v=fillWithMaskPiece(v, rg.start, rg.end);
var i, l=v.length;
for (i=rg.start; i < rg.end; i++) {
if (cvt.isMarker(getCChar(i))) break;
}
if (i < rg.end)
v=v.substr(0,i) + ch + v.substr(i+1);
state.setPos(rg.start+1);
} else {
v=v.substr(0,pos) + ((ch==hX_6._B)?promptChar:ch) + v.substr(pos+1);
pos=getMoveForward (v, pos+1);
state.setPos(pos);
}
state.setValue(v);
return -2;
}
function doCaretPrev (evt) {
if (evt.shiftKey || state.hasSel())
return -1;
else {
var pos=getMovePrevious (state.getValue(), state.getPos()-1);
state.setPos(pos);
return -2;
}
}
function doCaretNext (evt) {
if (evt.shiftKey || state.hasSel())
return -1;
else {
var pos=getMoveForward (state.getValue(), state.getPos()+1);
state.setPos(pos);
return -2;
}
}
function getMovePrevious (v, pos) {
var i, c, c1, l=v.length;
pos=(pos < 0) ? 0 : ((pos > l) ? l : pos);
for (i=pos; i > 0; i--) {
c=getCChar(i);
c1=getCChar(i-1);
if (cvt.isMarker(c) || cvt.isMarker(c1))
return (i);
}
return (0);
}
function getMoveForward(v, pos) {
var i, l=v.length;
pos=(pos < 0) ? 0 : ((pos > l) ? l : pos);
for (i=pos; i <= l; i++) {
if (cvt.isMarker(getCChar(i))) return (i);
}
return (l);
}
function  fillWithMaskPiece (v, start, end) {
return (v.substr(0,start) + emptyMask.substr(start, end-start) + v.substr(end));
}
function onClick(evt) {
if (state.hasSel())
return true;
var v=state.getValue(), pos=state.getPos();
if (pos <= 0 || pos > (v.length-1))
return true;
var cL=getCChar(pos-1), cR=getCChar(pos);
if (cvt.isMarker(cL) || cvt.isMarker(cL))
return true;
var MoveR=true, posLeft=getMovePrevious (v, pos), posRight=getMoveForward(v, pos);
if ((pos-posLeft) < (posRight-pos))
state.setPos(posLeft);
else
 state.setPos(posRight);
return -2;
}
function onChangeKeyCode (evt) {
switch (hX_6.getKeyCode(evt)) {
case hX_6.KEY_BS:
case hX_6.KEY_DEL:
return doDelete(evt);
case hX_6.KEY_LTARROW:
return doCaretPrev(evt);
case hX_6.KEY_RTARROW:
return doCaretNext(evt);
}
return true;
}
function onCut(evt) {
if (hX_6.brw.isIE() && state.hasSel()) {
var cutText=hX_6.sel.getSelection(elem);
window.clipboardData.setData("Text", cutText);
return doDelete(evt);
}
return true;
}
function onPaste (evt) {
if (hX_6.brw.isIE()) {
var cutText=window.clipboardData.getData("Text");
var v=state.getValue(), pos=state.getPos(), sel=state.hasSel();
if (sel) {
var rg=hX_6.sel.getSelectionR(elem);
v=(rg.start <= 0 && rg.end >= v.length-1)?emptyMask:fillWithMaskPiece(v, rg.start, rg.end);
}
var mChars=false, c, m, i, r, l=cutText.length;
for (i=0; i < l; i++) {
m=getCChar(i);
if (!cvt.isMarker(m)) {
mChars=(m == cutText.charAt(i));
break;
}
}
for (i=0; i < l; i++) {
m=getCChar(i);
c=cutText.charAt(i);
if (!cvt.isMarker(m) && m == c) {
} else {
c=(c==hX_6._B)?promptChar:c;
v=v.substr(0,pos) + c + v.substr(pos+1);
}
pos=(mChars) ? pos+1 : getMoveForward (v, pos+1);
}
state.setValue(v);
pos=getMoveForward (v, pos);
state.setPos(pos);
return -2;
}
return true;
}
function onChangeChrCode (evt) {
var v=state.getValue(), pos=state.getPos(), sel=state.hasSel(), ch=hX_6.getChrCode(evt);
if (evt.keyCode == hX_6.KEY_TAB || evt.keyCode == hX_6.KEY_ENTER)
return +2;
return (doInsert(v, ch, pos, sel));
}
}
HxG_6.prototype.POP_STYLE_DRAG=0;
HxG_6.prototype.POP_STYLE_CLICK= 1;
HxG_6.prototype.POP_STYLE_MANAGED= 2;
HxG_6.prototype.POP_STYLE_CLICK_SCROLLABLE= 3;
HxG_6.prototype.POP_DIR_RIGHT=1;
HxG_6.prototype.POP_DIR_CENTER=2;
HxG_6.prototype.JSFPopup=function (parentid, ownerobj, style, altComp) {
this.type="popup";
this.parentId=parentid;
this.DOMobj=this.parentDOMobj=null;
this.parentCOMPobj=(altComp!=null && altComp.type!=undefined)?altComp:null;
this.ownerDoc=document;
this.ownerObj=(ownerobj != null) ? ownerobj : document.body;
this.clickStyle=(style != null) ? style : hX_6.POP_STYLE_CLICK;
this.fullStyle=(this.clickStyle==hX_6.POP_STYLE_MANAGED);
this.sticky=false;
this.hideArray=this.hideArrayV=null;
this.dropShadowWidth=this.dropShadowColor=null;
this.dropDivs=[];
this.transparency=this.transDiv=null;
this.getElem=function getElem() { return this.DOMobj; }
this.getDoc=function getDoc()  { return this.ownerDoc; }
}
HxG_6.prototype.JSFPopup.prototype.onPageLoad=function() {
this.parentDOMobj=hX_6.getElementById(this.parentId);
this.parentCOMPobj=(this.parentCOMPobj!=null)?this.parentCOMPobj:hX_6.getComponentById(this.parentId);
if (this.parentDOMobj) {
if (!this.HTMLrendered) {
var elem=document.createElement(hX_6._DIV);
elem.setAttribute(hX_6._ID, this.id);
elem.setAttribute(hX_6._NAM, this.id + "_HXPOPUP");
elem.style.position=hX_6._ABS;
elem.style.left=elem.style.top=hX_6._ZPX;
elem.style.height=elem.style.width="10" + hX_6._PX;
elem.style.zIndex="1000000";
if (!this.fullStyle) elem.style.margin=elem.style.padding=hX_6._ZPX;
elem.style.verticalAlign=hX_6._TOP;
elem.style.overflow=hX_6._HI;
elem.style.display=hX_6._NO;
this.ownerObj.insertBefore (elem, (this.ownerObj.firstChild));
this.DOMobj=elem;
this.HTMLrendered=true;
return true;
}
}
return false;
}
HxG_6.prototype.JSFPopup.prototype.destroy=function() {
if (this.DOMobj) {
this.DOMobj.parentNode.removeChild(this.DOMobj);
delete this.DOMobj;
}
if (this.transDiv) {
this.transDiv.parentNode.removeChild(this.transDiv);
delete this.transDiv;
}
if (this.dropDivs) {
var e;
while (this.dropDivs.length > 0) {
e=this.dropDivs.pop();
e.parentNode.removeChild(e);
delete e;
}
}
if (this.hideArray) {
while (this.hideArray.length>0) this.hideArray.pop();
delete this.hideArray;
}
if (this.hideArrayV) {
while (this.hideArrayV.length>0) this.hideArrayV.pop();
delete this.hideArrayV;
}
return true;
}
HxG_6.prototype.JSFPopup.prototype.uivisible=function (dir, location, inline, absPt, overrideESC) {
var pop=this.DOMobj;
if (this.isPopped()) this.uirelease();
if (inline) {
if (pop) {
this.parentDOMobj=this.parentCOMobj=inline;
this.parentDOMobj.insertBefore(pop, this.parentDOMobj.firstChild);
pop.style.position="static";
pop.style.left=pop.style.top=hX_6._ZPX;
pop.style.visibility=hX_6._VS;
pop.style.display=hX_6._E;
}
} else {
if (pop && this.parentDOMobj) {
this.ownerObj.insertBefore (pop, this.ownerObj.firstChild);
var tagtop, tagleft, i, ds, off, poptop, popleft;
var base=(location != null) ? location : this.parentDOMobj;
var tagheight=hX_6.g.getRendHeight(base), tagwidth=hX_6.g.getRendWidth(base);
if (absPt) {
tagtop=poptop=absPt.Y; popleft=tagLeft=absPt.X;
} else {
if (hX_6.brw.isIE()) {
var p;
if (base.tagName==hX_6._SPN && base.parentNode && base.parentNode.tagName && base.parentNode.tagName==hX_6._TD) {
p=hX_6.g.getP(base.parentNode,hX_6._POS);
if (p.toLowerCase()==hX_6._REL || p.toLowerCase()=="static") base.parentNode.style.position="static";
}
if (hX_6.brw.isIE() && base.tagName && base.tagName==hX_6._TD) {
p=hX_6.g.getP(base,hX_6._POS);
if (p.toLowerCase()==hX_6._REL || p.toLowerCase()=="static") base.style.position="static";
}
}
tagtop=hX_6.g.getAbsPosTop(base); tagleft=hX_6.g.getAbsPosLeft(base);
if (dir != null && dir == hX_6.POP_DIR_RIGHT) {
poptop=tagtop - 1;
popleft=tagleft + tagwidth - 1;
} else {
poptop=tagtop + tagheight - 1;
popleft=tagleft;
}
}
pop.style.top=poptop + hX_6._PX;
pop.style.left= popleft + hX_6._PX;
pop.style.display=hX_6._BLK;
pop.style.visibility=hX_6._HI;
var x, y, h=hX_6.g.getRendHeight(pop), w=hX_6.g.getRendWidth(pop), popTopOff, popLeftOff;
var winheight=hX_6.g.getWindowClientHeight(), winwidth=hX_6.g.getWindowClientWidth();
var wintop=Math.max(document.body.scrollTop,document.body.parentNode.scrollTop),
winleft=Math.max(document.body.scrollLeft,document.body.parentNode.scrollLeft);
if (((poptop-wintop) + h > winheight) && (winheight > h)) {
if ((tagtop-h+1)>=0) pop.style.top=(tagtop - h + 1) + hX_6._PX;
}
if (dir != null && dir==hX_6.POP_DIR_CENTER) {
popleft+=((tagwidth - w)/2);
pop.style.left= popleft + hX_6._PX;
}
if (popleft < winleft) pop.style.left= winleft + hX_6._PX;
if (popleft+w >= (winleft+winwidth-1)) pop.style.left= (tagleft + tagwidth - w) + hX_6._PX;
y=hX_6.g.getAbsPosTop(pop); x=hX_6.g.getAbsPosLeft(pop);
popTopOff=popLeftOff=0;
if (pop.offsetParent && pop.offsetParent.tagName) {
poptop=parseInt(pop.style.top,10); popleft=parseInt(pop.style.left,10);
if (y!=poptop) {
y=hX_6.g.getAbsPosTop(pop.offsetParent);
pop.style.top=(poptop-y)+hX_6._PX;
y=hX_6.g.getAbsPosTop(pop);
}
if (x!=popleft){
x=hX_6.g.getAbsPosLeft(pop.offsetParent);
pop.style.left=(popleft-x)+hX_6._PX;
x=hX_6.g.getAbsPosLeft(pop);
}
}
popTopOff=parseInt(pop.style.top,10);
popLeftOff=parseInt(pop.style.left,10);
if (hX_6.brw.isIE() && pop.filters && pop.filters.length > 0) {
for (i=0; i < pop.filters.length; i++)
if (hX_6.isDef(typeof pop.filters[i].Apply)) pop.filters[i].Apply();
}
pop.style.visibility=hX_6._VS;
if (hX_6.brw.isIE() && pop.filters && pop.filters.length > 0) {
for (i=0; i < pop.filters.length; i++)
if (hX_6.isDef(typeof pop.filters[i].Play)) pop.filters[i].Play();
}
var t=hX_6.g.getP(pop, "opacity");
this.transparency=(t < 1) ? t : null;
if (this.transparency && this.transDiv == null) {
ds=this.transDiv=document.createElement(hX_6._DIV);
ds.style.position=hX_6._ABS;
ds.style.display=hX_6._NO;
ds.style.padding=this.transDiv.style.margin=this.transDiv.style.left=this.transDiv.style.top=hX_6._ZPX;
ds.style.backgroundColor=hX_6.g.getP(pop, hX_6._BCOR);
ds.style.backgroundImage=hX_6.g.getP(pop, hX_6._BIMG);
ds.style.backgroundRepeat=hX_6.g.getP(pop, hX_6._BREP);
ds.style.backgroundAttachment=hX_6.g.getP(pop, "background-attachment");
if (hX_6.brw.isIE()) {
ds.style.backgroundPositionX=hX_6.g.getP(pop, "background-position-x");
ds.style.backgroundPositionY=hX_6.g.getP(pop, "background-position-y");
} else {
ds.style.backgroundPosition=hX_6.g.getP(pop, hX_6._BPOS);
}
pop.insertBefore (ds, pop.firstChild);
}
if (this.transparency && this.transDiv) {
ds=this.transDiv;
var lB=hX_6.g.parseBorder(pop, hX_6._LFT,0), rB=hX_6.g.parseBorder(pop, hX_6._RGT,0);
var tB=hX_6.g.parseBorder(pop, hX_6._TOP,0),  bB=hX_6.g.parseBorder(pop, hX_6._BOT,0);
ds.style.left=ds.style.top=hX_6._ZPX;
ds.style.width=(w-(lB+rB)) + hX_6._PX;
ds.style.height=(h-(tB+bB)) + hX_6._PX;
ds.style.display=hX_6._BLK;
ds.style.zIndex=hX_6.__1;
pop.style.backgroundColor=hX_6._TRN;
pop.style.backgroundImage=hX_6._NO;
if (hX_6.brw.isNavOrMoz()) pop.style.opacity=hX_6._1;
this.addTransparency(ds, this.transparency);
}
var offX, offY, mgnTop, mgnLft;
if (hX_6.brw.isNavOrMoz()) {
pop.style.MozOutlineStyle=hX_6._SLD;
this.dropShadowWidth=parseInt(hX_6.g.getP(pop, "-moz-outline-width"),10);
pop.style.MozOutlineStyle=hX_6._E;
this.dropShadowColor=hX_6.g.getP(pop, "-moz-outline-color");
mgnTop=parseInt(hX_6.g.getP(pop, "margin-top"),10);
mgnLft=parseInt(hX_6.g.getP(pop, "margin-left"),10);
offY=mgnTop-1;
offX=mgnLft-1;
} else {
this.dropShadowWidth=parseInt(hX_6.g.getP(pop, null, "outline-width"),10);
this.dropShadowColor=hX_6.g.getP(pop, null, "outline-color");
mgnTop=parseInt(hX_6.g.getP(pop, "margin-top"),10);
mgnLft=parseInt(hX_6.g.getP(pop, "margin-left"),10);
offY=mgnTop;
offX=mgnLft;
}
if (this.dropShadowWidth > 0) {
this.dropShadowWidth=(this.dropShadowWidth > 10) ? 10 : this.dropShadowWidth;
var scale=(this.transparency) ? this.transparency : .8;
var scaleStep=(scale-0.10)/this.dropShadowWidth;
scaleStep=(scaleStep < 0) ? 0 : scaleStep;
for (i=1; i <= this.dropShadowWidth; i++) {
if (i > this.dropDivs.length) {
ds=document.createElement(hX_6._DIV);
ds.style.position=hX_6._ABS;
ds.style.display=hX_6._NO;
ds.style.padding=ds.style.margin=ds.style.left=ds.style.top=hX_6._ZPX;
ds.style.backgroundColor=hX_6._TRN;
ds.style.borderStyle=hX_6._SLD;
ds.style.borderWidth=hX_6._ZPX;
ds.style.borderBottomWidth=ds.style.borderRightWidth=hX_6._1PX;
this.dropDivs.push(ds);
this.ownerObj.insertBefore (ds, (this.ownerObj.firstChild));
}
ds=this.dropDivs[i-1];
ds.style.borderColor=(this.dropShadowColor) ? this.dropShadowColor : hX_6.CLR_SHDW;
this.addTransparency(ds, (scale-((i-1)*scaleStep)));
}
}
for (i=0; i < this.dropShadowWidth; i++) {
ds=this.dropDivs[i];
if (ds) {
ds.style.left=(popLeftOff+i+2+offX) + hX_6._PX;
ds.style.top=(popTopOff+i+2+offY) + hX_6._PX;
ds.style.width=(w-1) + hX_6._PX;
ds.style.height=(h-1) + hX_6._PX;
ds.style.zIndex=hX_6._E+(1000000-11+i);
ds.style.display=hX_6._BLK;
}
}
if (hX_6.brw.isIE() && !hX_6.brw.isIE7() && document.elementFromPoint) {
var x1, y1, test, sBase=(hX_6.brw.isIE6_CSS()) ? document.body.parentNode : document.body;
x=x - sBase.scrollLeft;
y=y - sBase.scrollTop;
pop.style.display=hX_6._NO;
for (x1=x; x1 < x+w; x1+=12) {
for (y1=y; y1 < y+h; y1+=12) {
test=document.elementFromPoint(x1, y1);
if (test && test.tagName == hX_6._SEL && test.style.visibility != hX_6._HI && test.style.display != hX_6._NO) {
if (!this.hideArray) {
this.hideArray=[];
this.hideArrayV=[];
}
this.hideArray.push(test);
this.hideArrayV.push(test.style.visibility);
test.style.visibility=hX_6._HI;
}
}
test=document.elementFromPoint(x1, y+h-1);
if (test && test.tagName == hX_6._SEL && test.style.visibility != hX_6._HI && test.style.display != hX_6._NO) {
if (!this.hideArray) {
this.hideArray=[];
this.hideArrayV=[];
}
this.hideArray.push(test);
this.hideArrayV.push(test.style.visibility);
test.style.visibility=hX_6._HI;
}
}
pop.style.display=hX_6._BLK;
}
}
}
if (pop && this.parentCOMPobj && this.clickStyle != hX_6.POP_STYLE_MANAGED) {
hX_6.popMgr.start (pop, this.parentCOMPobj, (this.clickStyle==hX_6.POP_STYLE_CLICK_SCROLLABLE));
if (overrideESC && hX_6.brw.isNavOrMoz()) hX_6.popMgr.overrideESC=true;
}
return true;
}
HxG_6.prototype.JSFPopup.prototype.uirelease=function (force) {
if (this.DOMobj && (force || !this.sticky)) {
var i;
for (i=0; i < this.dropShadowWidth; i++)
if (this.dropDivs[i]) this.dropDivs[i].style.display=hX_6._NO;
if (this.transparency && this.transDiv) {
this.transDiv.style.display=hX_6._NO;
this.DOMobj.style.backgroundColor=this.DOMobj.style.backgroundImage=hX_6._E;
if (hX_6.brw.isNavOrMoz()) this.DOMobj.style.opacity=hX_6._E;
}
this.DOMobj.style.display=hX_6._NO;
this.DOMobj.style.visibility=hX_6._HI;
if (this.hideArray) {
for (i=0; i < this.hideArray.length; i++) {
if (this.hideArray[i] && this.hideArray[i].style)
this.hideArray[i].style.visibility=this.hideArrayV[i];
}
this.hideArray=null;
}
return true;
}
return false;
}
HxG_6.prototype.JSFPopup.prototype.isPopped=function() {
return (this.DOMobj && this.DOMobj.style.display != hX_6._NO) ? true : false;
}
HxG_6.prototype.JSFPopup.prototype.addTransparency=function(o,t) {
if (o) {
if (o.filters) {
o.style.filter="progid:DXImageTransform.Microsoft.Alpha(enabled=false)";
o.filters.item("DXImageTransform.Microsoft.Alpha").opacity=t*100;
o.filters.item("DXImageTransform.Microsoft.Alpha").enabled=true;
} else if (o.style)
o.style.MozOpacity=t;
}
}
HxG_6.prototype.JSFPopup.prototype.removeTransparency=function(o) {
if (o) {
if (o.filters) {
o.style.filter="progid:DXImageTransform.Microsoft.Alpha(enabled=false)";
o.filters.item("DXImageTransform.Microsoft.Alpha").opacity=100;
o.filters.item("DXImageTransform.Microsoft.Alpha").enabled=true;
} else if (o.style)
o.style.MozOpacity=1;
}
}
HxG_6.prototype.tabNavigator=function() {
this.getFocus=function (obj, dir, h, scope, ignVis) { return gFocus(obj,  ((dir > 0)?+1:-1), h, scope, ignVis); }
this.getTabItems= function () { return tabItems; }
this.getFirst=getFirst;
this.getLast=getLast;
this.firstInScope=this.lastInScope=this.scope=null;
var i, tabItems=[];
function elem(obj, s) {
this.obj=obj;
this.name=(obj.name) ? obj.name : hX_6._E;
this.tabOrder=obj.getAttribute("tabIndex");
this.tabOrder=(this.tabOrder==null)?0:this.tabOrder;
this.srcOrder=s;
}
collectTabElement(document.body);
for (i=0; i < hX_6.HTMLTags.length; i++) collectTabElement(hX_6.HTMLTags[i]);
tabItems.sort(compareTabIndex);
function collectTabElement(iNode) {
var x, child, node=iNode;
if (node.tagName && node.tagName == hX_6._IFRM) {
try {
node=(node.contentDocument) ? node.contentDocument : ((node.contentWindow) ? node.contentWindow.document.body : null);
x=(node.nodeType);
}
catch(e) {
node=null;
}
}
if (node!=null) {
if (hX_6.isTabbable(node)) if (notDuplicateElement(node)) tabItems.push(new elem(node, tabItems.length));
if (!node.tagName || (node.tagName && node.tagName!=hX_6._SEL)) {
for (child=node.firstChild; child != null; child=child.nextSibling) collectTabElement(child);
}
}
}
function notDuplicateElement(node) {
if (node && node.nodeType!=undefined && node.nodeType == hX_6.ELEMENT_NODE) {
if (node.tagName && node.tagName == hX_6._INP && node.type && node.type.toLowerCase() == hX_6._RAD) {
if (node.name && node.name.length > 0) {
var ix, l=tabItems.length;
for (ix=0; ix<l; ix++) if (tabItems[ix].name == node.name) return false;
}
}
}
return true;
}
function compareTabIndex(ia, ib) {
var r=0, a=ia.tabOrder, b=ib.tabOrder;
if ((a == b) || (a < 0 && b < 0))	r=0;
else if (a > 0 && b > 0)			r=a-b;
else if (a >= 0 && b <= 0)			r=-1;
else if (b >= 0 && a <= 0)			r=+1;
return ((r != 0)? r : (ia.srcOrder - ib.srcOrder));
}
function getFirst(scope, skipVis) {
var r=null, sV=(skipVis!=null && skipVis);
if (tabItems.length==0)	r=null;
else {
if (scope!=null && this.scope==scope && this.firstInScope!=null)
r=this.firstInScope;
else {
if ((tabItems[0]).tabOrder>=0 && (sV || hX_6.i.isFocusable(tabItems[0].obj)) && (scope==null || hX_6.isChildTag(tabItems[0].obj, scope)))
r=(tabItems[0].obj);
else
 r=gFocus(tabItems[0].obj, 1, false, scope, sV);
if (scope!=null) {
if (this.scope!=scope) this.lastInScope=null;
this.scope=scope;
this.firstInScope=r;
}
}
}
return r;
}
function getLast(scope, skipVis) {
var l=tabItems.length-1, r=null, sV=(skipVis!=null && skipVis);
if (tabItems.length==0)	r=null;
else {
if (scope!=null && this.scope==scope && this.lastOutScope!=null) r=this.lastInScope;
else {
if ((tabItems[l]).tabOrder >= 0 && (sV || hX_6.i.isFocusable(tabItems[l].obj)) && (scope==null || hX_6.isChildTag(tabItems[l].obj, scope)))
r=(tabItems[l].obj);
else
 r=gFocus(tabItems[l].obj, -1, false, scope, sV);
if (scope!=null) {
if (this.scope!=scope) this.firstInScope=null;
this.scope=scope;
this.lastInScope=r;
}
}
}
return r;
}
function gFocus(obj, delta, skipH, inscope, skipVis) {
var r=null, ix, hit=false;
for (ix=0;ix<tabItems.length; ix++) {
if (tabItems[ix].obj == obj) { hit=true; break; }
}
if (hit) {
r=(tabItems[ix]).obj;
var obj0=r, ix0=ix+delta, i=0;
hit=false;
while (ix!=ix0 && !hit && i<tabItems.length) {
if (ix0 < 0) ix0=tabItems.length-1;
if (ix0 >= tabItems.length)	ix0=0;
if (!skipH || tabItems[ix0].obj.id==undefined || !(skipH && tabItems[ix0].obj.id.length>10 && tabItems[ix0].obj.id.substr(tabItems[ix0].obj.id.length-10)=="_HXB_HPBTN")) {
obj0=(tabItems[ix0]).obj;
if ((tabItems[ix0]).tabOrder >= 0)
hit=(inscope==null || (inscope!=null  && hX_6.isChildTag(obj0, inscope))) && (skipVis || hX_6.i.isFocusable(obj0));
}
ix0+=delta; i++;
}
r=(hit) ? obj0 : r;
}
return r;
}
}
HxG_6.prototype.inputState=function (_obj, _behavior, _shortform) {
var obj=_obj, behavior=_behavior, value=obj.value, orgvalue=obj.value, focusToValue=null;
var pos=-1, sel=-1, orgpos=-1, autotab=0, skipC=false;
var keydown=false, keypress=false, updating=false, autosubmit=false, focus=false, checkedComponent=false;
var keypress_r=null, commitCallback=null, hasComponent=null, autotabtimer=null;
this.rollback=rollback;
this.commit=commit;
this.uirelease=uirelease;
this.destroy=destroy;
this.setAutoTab=function setAutoTab(x)	{ autotab=x;			}
this.setAutoSubmit= function setAutoSubmit(){ autosubmit=true;	}
this.setCommitCallback= function setCommitCallback(x) { commitCallback=x; }
this.setValue=function setValue(c) 	{ value=c;			}
this.getValue=function getValue()		{ return value;			}
this.setPos=function setPos(c)		{ pos=c;				}
this.getPos=function getPos()		{ return pos;			}
this.hasSel=function hasSel()		{ return sel;			}
this.dispatch=dispatch;
this.dispatchTimer=dispatchTimer;
hX_6.attachInputStateEvent(obj, hX_6._onF);
hX_6.attachInputStateEvent(obj, hX_6._onB);
hX_6.attachInputStateEvent(obj, hX_6._onKD);
hX_6.attachInputStateEvent(obj, hX_6._onKP);
hX_6.attachInputStateEvent(obj, hX_6._onKU);
if (_shortform == null) {
hX_6.attachInputStateEvent(obj, hX_6._onMC);
if (hX_6.brw.isIE()) {
hX_6.attachInputStateEvent(obj, hX_6._onCP);
hX_6.attachInputStateEvent(obj, hX_6._onCT);
hX_6.attachInputStateEvent(obj, hX_6._onPST);
}
}
function saveState () {
value=obj.value;
pos=hX_6.sel.getCaretPos(obj);
sel=hX_6.sel.hasSelection(obj);
orgvalue= value;
orgpos=pos;
}
function dispatchTimer (item, action) {
autotabtimer=null;
if (focus && action == "autotab" && (autotab > 0)) doAutoTab();
}
function destroy () {
hX_6.removeAllEvents(obj, obj.id);
}
function uirelease () {
keydown=keypress=false;
updating=true;
if (autotabtimer) {
hX_6.i.killTimerTimed(autotabtimer);
autotabtimer=null;
}
if (focus && behavior.assist && behavior.assist.onBlur) behavior.assist.onBlur(null);
focus=false;
hX_6.pendingPrompt=null;
}
function dispatch (evt) {
var r=true, newR;
if (hX_6.pendingFocusReturn && hX_6.pendingFocusReturn != obj.id) return true;
hX_6.pendingFocusReturn=null;
if ((obj.readOnly != null && obj.readOnly == true) || (obj.disabled != null && obj.disabled == true)) return true;
skipC=false;
switch (evt.type) {
case hX_6._evF:
focus=true;
hX_6.pendingPrompt=this;
keydown=keypress=false;
updating=true;
autotabtimer=null;
if (!checkedComponent) {
hasComponent=hX_6.getComponentById(obj.id);
checkedComponent=true;
}
if (behavior.assist && behavior.assist.onFocus) behavior.assist.onFocus(evt);
if (hX_6.pendingSelect && obj.select) obj.select();
hX_6.pendingSelect=false;
saveState();
if (behavior.assist && pos >= 0) obj.style.imeMode=hX_6._DSB;
focusToValue=value;
break;
case hX_6._evB:
keydown=keypress=false;
updating=true;
if (autotabtimer) {
hX_6.i.killTimerTimed(autotabtimer);
autotabtimer=null;
}
if (focus && behavior.assist && behavior.assist.onBlur) behavior.assist.onBlur(evt);
if (focus && behavior.assist && focusToValue != value) hX_6.i.fireEvent (obj, hX_6._onC);
focus=false;
hX_6.pendingPrompt=null;
break;
case hX_6._evKD:
if (!focus) break;
keydown=true;
keypress=updating=false;
saveState();
if (isDisposableKey(evt) && behavior.assist) return false;
else if (isSubmitFcnKey(evt) && behavior.assist) {
if (focus && behavior.assist && behavior.assist.onBlur) behavior.assist.onBlur(evt);
focus=false;
break;
} else if (hX_6.isFcnKey(evt) && behavior.assist) {
if (hasComponent && hasComponent.isRedirectFcnKey && hasComponent.isRedirectFcnKey(evt)) {
var newValue=(behavior.assist.fromAssist) ? behavior.assist.fromAssist(value) : obj.value;
newValue=(hasComponent.doRedirectFcnKey) ? hasComponent.doRedirectFcnKey(evt, newValue) : newValue;
newValue=(behavior.assist.toAssist) ? behavior.assist.toAssist(newValue) : newValue;
value=(newValue.v) ? newValue.v : value;
commit(evt);
if (pos >= 0) hX_6.sel.setCaretPos(obj, pos);
r=false;
} else if (behavior.assist.onChangeKeyCode && !isSkipFcnKey(evt)) {
r=behavior.assist.onChangeKeyCode(evt);
if (r == -1) r=true;
else if (r) {
newR=doChange(evt);
r=(r == -2) ? false : newR;
}
if (!r) hX_6.i.cancelAction(evt, true);
}
if (!isPrintableFcnKey(evt)) {
keypress=true;
keypress_r=r;
}
}
break;
case hX_6._evKP:
if (!focus) break;
updating=false;
if (hX_6.brw.isNavOrMoz() && evt.keyCode && (evt.keyCode == 191 || evt.keyCode == 229) && evt.charCode!=null && evt.charCode==0)
return false;
if (keypress)
r=keypress_r;
else {
keypress=true;
if (behavior.assist && behavior.assist.onChangeChrCode) {
r=behavior.assist.onChangeChrCode(evt);
newR=false;
if (r > 1) {
r=true;
newR=true;
} else if (r) {
newR=doChange(evt);
r=(r == -2) ? false : newR;
}
if (newR && (autotab > 0)) doAutoTab();
} else if (autotab > 0) autotabtimer=hX_6.i.startTimerTimed (obj.id, 0, "autotab", 1, hX_6.BHR_assist);
}
break;
case hX_6._evKU:
if (!focus) break;
updating=false;
if (behavior.assist && behavior.assist.onChangeKeyCode && (keydown && !keypress)) {
if (hX_6.brw.isIE() && evt.keyCode == 67)
r=false;
else {
r=behavior.assist.onChangeKeyCode(evt);
if (r) {
newR=doChange(evt);
r=(r == -2) ? false : newR;
}
}
}
keydown=keypress=false;
break;
case hX_6._evMC:
if (!focus) break;
keydown=keypress=updating=false;
saveState();
if (!sel) {
if (behavior.assist && behavior.assist.onClick) {
r=behavior.assist.onClick(evt);
if (r == -2 && pos >= 0) {
hX_6.sel.setCaretPos(obj, pos);
r=false;
}
}
}
break;
case hX_6._evCT:
if (!focus) break;
keydown=keypress=updating=false;
saveState();
if (sel) {
if (behavior.assist && behavior.assist.onCut) {
r=behavior.assist.onCut(evt);
if (r == -1) r=true;
else if (r) {
r=doChange(evt);
r=(r == -2) ? false : r;
}
}
}
break;
case hX_6._evCP:
if (focus && sel && behavior.assist && behavior.assist.onCopy) behavior.assist.onCopy(evt);
break;
case hX_6._evPST:
if (!focus) break;
keydown=keypress=updating=false;
saveState();
if (behavior.assist && behavior.assist.onPaste) {
newR=false;
r=behavior.assist.onPaste(evt);
if (r) {
newR=doChange(evt);
r=(r == -2) ? false : newR;
}
var didTab=false;
if (newR && (autotab > 0)) didTab=doAutoTab();
if (hX_6.brw.isIE() && newR && !didTab && pos >= 0) hX_6.sel.setCaretPos(obj, pos, null, true);
}
break;
}
if (r==false && hX_6.brw.isIE() && evt.type==hX_6._evKP) evt.returnValue=false;
return r;
}
function doChange(evt) {
var r=true;
if (behavior.assist && behavior.assist.onValidate) r=behavior.assist.onValidate(evt);
if (r) commit(evt);
else rollback(evt);
return r;
}
function rollback (evt) {
value=orgvalue;
pos=orgpos;
update(evt);
}
function commit (evt) {
if (!skipC && commitCallback) commitCallback(obj);
update (evt);
}
function update (evt) {
if (!updating) {
updating=true;
if (obj.value != value) obj.value=value;
if (pos >= 0) hX_6.sel.setCaretPos(obj, pos);
updating=false;
}
}
function doAutoTab() {
if (isFilled(hX_6.sel.getCaretPos(obj))) {
if (autosubmit) {
var a=[], t=[];
a.push(hX_6.ACT_SUBMIT);
t.push((behavior.autoSubmit.length > 0) ? behavior.autoSubmit : obj.form.id);
return (hX_6.action.processAction (obj, null, null, a, true, t, null, null, null, true));
} else
 hX_6.gotoNextPrev(obj, true, true);
return true;
}
return false;
}
function isFilled(curpos) {
return ((behavior && behavior.assist && behavior.assist.isFilled)?behavior.assist.isFilled(curpos):(obj.value.length >= autotab && (curpos >= autotab)));
}
function isSubmitFcnKey (evt) {
var code=(evt.keyCode && (evt.keyCode != 0)) ? evt.keyCode : -1;
if (code == 13)	if (!hX_6.isKeyBound(obj, "13", evt, hX_6.ACT_SUBMIT)) return true;
return false;
}
function isDisposableKey (evt) {
var code=(evt.keyCode && (evt.keyCode != 0)) ? evt.keyCode : -1;
return (hX_6.isKeyBound(obj, code+hX_6._E, evt, hX_6.ACT_NOTHING, hX_6.ACT_NONE, hX_6.ACT_STOP));
}
function isSkipFcnKey (evt) {
var code=(evt.keyCode && (evt.keyCode != 0)) ? evt.keyCode : -1;
if (code == 16 || code == 17 || code == 18 ||
code == 145 || code == 166 || code == 167)
return true;
return false;
}
function isPrintableFcnKey (evt) {
var code=(evt.keyCode && (evt.keyCode != 0)) ? evt.keyCode : -1;
return (code >= 223);
}
}
hX_6.STR_SPINNER_INCREMENT="Increment";
hX_6.STR_SPINNER_DECREMENT="Decrement";
hX_6.STR_TABPANEL_SCROLLLEFT="Scroll Left";
hX_6.STR_TABPANEL_SCROLLRIGHT="Scroll Right";
