}
HxG_6.prototype.DateTimeConverter.prototype.setAttribute=function(a) {
var i,j,k;
hX_6.parseArg (this.attributeTable, this, a);
if (this.locale && this.locale != this.lc) {
this.lc=this.locale;
hX_6.c.strInit(this.lc);
this.sAM=hX_6.L[this.lc].STR_AM_SUFFIX;
this.sPM=hX_6.L[this.lc].STR_PM_SUFFIX;
}
this.FMTChars=this.FMTBaseChars + ((this.ICU4J)?this.FMTICU4JChars:hX_6._E);
this.FMTDateChars=this.FMTBaseDateChars + ((this.ICU4J)?this.FMTICU4JDateChars:hX_6._E);
this.FMTTimeChars=this.FMTBaseTimeChars + ((this.ICU4J)?this.FMTICU4JTimeChars:hX_6._E);
this.parseDigits();
this.parseFormatString();
this.sEpoch=(this.sEpoch==null)?hX_6._E:this.sEpoch; this.nEpoch=0;
for (i=0; i < hX_6.DATE_EPOCH_IDS.length;  i++) {
if (hX_6.DATE_EPOCH_IDS[i] == this.sEpoch) {
this.nEpoch=i;
break;
}
}
this.bIsGregorian=this.bIsHijiri=this.bIsHebrew=this.bIsChristianEra=this.bIsBuddhistEra=this.bIsJapaneseEra=false;
this.bIsChristianEra=(this.nEpoch <= 0);
this.bIsHijiri=(this.nEpoch == 1);
this.bIsHebrew=(this.nEpoch == 2);
this.bIsBuddhistEra=(this.nEpoch == 3);
this.bIsJapaneseEra=(this.nEpoch == 4);
this.bIsGregorian=(this.nEpoch == 0 || this.nEpoch >= 3);
this.bShowEpoch=(this.bIsJapaneseEra || this.sFormatTokenized.indexOf("%G")>=0);
this.descLbEpoch=hX_6.L[this.lc].STRA_DATE_EPOCHS[this.nEpoch];
this.descYrEpoch=[];
this.descDtEpoch=[];
this.isShortYr=(this.descLbEpoch.length > 2);
this.setCalendar();
this.abbEpochLength=this.fulEpochLength=0; this.minEpoch=1;
this._AD=this.abbEpochLength,this.descLbEpoch[1][1];
for (i=0; i < this.descLbEpoch.length; i++) {
this.fulEpochLength=Math.max(this.fulEpochLength,this.descLbEpoch[i][0].length);
this.abbEpochLength=Math.max(this.abbEpochLength,this.descLbEpoch[i][1].length);
this.minEpoch=(i<=0)?this.minEpoch:((this.descLbEpoch[i-1][1].charAt(0)==this.descLbEpoch[i][1].charAt(0))?2:1);
if (this.bIsChristianEra) {
this.descYrEpoch.push(0);
this.descDtEpoch.push(0);
} else {
j=this.constantToValue(hX_6.DATE_EPOCH_YEARS[this.nEpoch][i]);
this.descYrEpoch.push((j!=null)?j.getFullYear():0);
this.descDtEpoch.push(j);
}
}
if (this.simpleFormat) this.genAssistMasks();
return true;
}
HxG_6.prototype.DateTimeConverter.prototype.epochIndex=function () {
return ((this.bIsHijiri)?1:((this.bIsHebrew)?2:0));
}
HxG_6.prototype.DateTimeConverter.prototype.FormatToken=function (token, typ) {
this.type=typ; this.token=token;
}
HxG_6.prototype.DateTimeConverter.prototype.parseDigits=function () {
this.xDigits=null; this.zeroDigit=hX_6._Z;
if (this.digitFirst != null) {
this.xDigits=String.fromCharCode(this.digitFirst, this.digitFirst+1, this.digitFirst+2, this.digitFirst+3, this.digitFirst+4, this.digitFirst+5, this.digitFirst+6, this.digitFirst+7, this.digitFirst+8, this.digitFirst+9);
this.zeroDigit=this.xDigits.charAt(0);
}
}
HxG_6.prototype.DateTimeConverter.prototype.parseFormatString=function () {
var fmt=this.sFormat;
this.sFormatTokenized=hX_6._E;
this.nLiteralCnt=this.nTokenCnt=this.DF_MaxLength=0;
var index, i, xchar, nchar, last=hX_6._E, lastlen=1, q=false, noT=false, len=fmt.length;
for (index=0; index < len; index++) {
xchar=fmt.charAt(index);
if (!q && hX_6.isChar(xchar, this.FMTChars)) {
if (xchar==last) {
noT=false;
if (lastlen >= 8) noT=true;
else if (lastlen >= 4 && !hX_6.isChar(xchar, this._8chars)) noT=true;
else if (lastlen >= 3 && !hX_6.isChar(xchar, this._4chars)) noT=true;
else if (lastlen >= 2 && !hX_6.isChar(xchar, this._3chars)) noT=true;
else if (lastlen >= 1 && !hX_6.isChar(xchar, this._2chars)) noT=true;
if (!noT) this.sFormatTokenized=this.sFormatTokenized + xchar;
lastlen++;
} else {
this.sFormatTokenized=this.sFormatTokenized + hX_6._PCT + xchar;
lastlen=1;
}
last=xchar;
this.bHasDate=(this.bHasDate) ? this.bHasDate : hX_6.isChar(xchar, this.FMTDateChars);
this.bHasTime=(this.bHasTime) ? this.bHasTime : hX_6.isChar(xchar, this.FMTTimeChars);
} else {
if (xchar == hX_6._SQT) {
nchar=fmt.charAt(index+1);
if (nchar == hX_6._SQT) {
index++;
} else if (q) {
xchar=hX_6._E;
q=false;
} else {
xchar=nchar;
q=true;
index++;
}
}
if (last != hX_6._UPC) {
this.sFormatTokenized=this.sFormatTokenized + hX_6._UPC + this.nLiteralCnt;
this.sLiterals[this.nLiteralCnt]=hX_6._E;
this.nLiteralCnt++;
}
this.sLiterals[this.nLiteralCnt-1]=this.sLiterals[this.nLiteralCnt-1] + xchar;
last=hX_6._UPC;
}
}
fmt=this.sFormatTokenized;
var token, next, nextp, nextl, c, l;
while (fmt.length > 0) {
xchar=fmt.charAt(0);
fmt=fmt.substr(1);
nextp=fmt.indexOf(hX_6._PCT);
nextl=fmt.indexOf(hX_6._UPC);
next=(nextp < 0) ? nextl : ((nextl < 0) ? nextp : Math.min(nextp,nextl));
if (next >= 0) {
token=fmt.substr(0,next);
fmt=fmt.substr(next);
} else {
token=fmt;
fmt=hX_6._E;
}
if (xchar==hX_6._PCT) {
this.sTokens[this.nTokenCnt]=new this.FormatToken(token, 0);
this.nTokenCnt++;
} else {
index=parseInt(token, 10);
token=this.sLiterals[index];
for (i=1; i < this.FMTSEPChars.length; i++)
token=hX_6.replaceAll(token, this.FMTSEPChars.charAt(i), this.FMTSEPChars.charAt(0));
token=hX_6.replaceAll(token,  this.FMTSEPChars.charAt(0)+ this.FMTSEPChars.charAt(0),  this.FMTSEPChars.charAt(0));
this.sTokens[this.nTokenCnt]=new this.FormatToken(this.sLiterals[index], (token == this.FMTSEPChars.charAt(0)) ? 2 : 1);
this.nTokenCnt++;
}
}
for (i=0; i < this.nTokenCnt; i++) {
if (this.sTokens[i].type == 0) {
l=this.sTokens[i].token.length;
switch (this.sTokens[i].token.charAt(0)) {
case hX_6._uF:  case hX_6._uW:  case hX_6._lE:
this.DF_MaxLength+=1;
break;
case hX_6._uD: case hX_6._uS:
this.DF_MaxLength+=3;
break;
case hX_6._uZ:
this.DF_MaxLength+=5;
break;
case hX_6._uA: case hX_6._lG:
this.DF_MaxLength+=8;
break;
case hX_6._lZ:
this.DF_MaxLength+=10;
break;
case hX_6._uG:
this.DF_MaxLength+=((l<=3)?this.abbEpochLength:this.fulEpochLength);
break;
case hX_6._lY: case hX_6._uY:
this.DF_MaxLength+=((this.bIsJapaneseEra)?l:((l==2)?2:4));
break;
case hX_6._uE:
this.DF_MaxLength+=((l > 3) ? hX_6.c.L[this.lc].MAX_STR_DATE_DAYS[this.epochIndex()] : hX_6.c.L[this.lc].MAX_STR_DATE_SHDAYS[this.epochIndex()]);
break;
case hX_6._uM:
this.DF_MaxLength+=((l < 3)?2:((l > 3) ? hX_6.c.L[this.lc].MAX_STR_DATE_MONTHS[this.epochIndex()] : hX_6.c.L[this.lc].MAX_STR_DATE_SHMONTHS[this.epochIndex()]));
break;
case hX_6._lA:
this.DF_MaxLength+=Math.max(this.sAM.length, this.sPM.length);
break;
default:
this.DF_MaxLength+=2;
break;
}
} else this.DF_MaxLength+=this.sTokens[i].token.length;
}
}
HxG_6.prototype.DateTimeConverter.prototype.getAttribute=function (attribute) {
return (hX_6.getArg(this.attributeTable, this, attribute));
}
HxG_6.prototype.DateTimeConverter.prototype.lastError=function () {
return ((this.msgFail && this.msgFail.length > 0) ? this.msgFail : null);
}
HxG_6.prototype.DateTimeConverter.prototype.stringToValue=function (iDate) {
var r=this.stringToValueBase(iDate);
return ((r) ? r.value : null);
}
HxG_6.prototype.DateTimeConverter.prototype.stringToValueBase=function (iDate, tokenStop) {
this.intMsgFail=this.msgFail=hX_6._E;
var sDate=iDate, args=[iDate];
if (iDate == null || !hX_6.isString(iDate)) {
this.msgFail=hX_6.L[this.plc].ERR_empty;
return null;
}
while (sDate.length > 0 && sDate.charAt(sDate.length-1) == hX_6._B)
sDate=sDate.substr(0, sDate.length-1);
if (sDate.length == 0)
return null;
this.setCalendar();
var tok=this.sTokens, tokI=0, curdate=new Date(), o=hX_6.cal.dateToCalendar(curdate);
var intMonth=o.month+1, intDay=o.date, intYear=o.year, intSvDay=o.date, intJDay=0;
var intHour=12, intMin=0, intSec=0, intMS=0, intMSDay=0, ampm=hX_6._E;
var ADtoCal=false, iEpoch=null, zISO=false, zOffset=null, bShortYear=false, bShortYearSpec=false, bShortHour=false, bKampm=false, bJulianDate=false, bNormalDate=false;
var bNumberInShMonth=this.matchDigit(hX_6.L[this.lc].STRA_DATE_SHMONTHS[this.epochIndex()][0]);
var bNumberInMonth=this.matchDigit(hX_6.L[this.lc].STRA_DATE_MONTHS[this.epochIndex()][0]);
var monthStrict=(bNumberInMonth || bNumberInShMonth)?2:this.nStrict;
var tokenC=((tokenStop != null) ? tokenStop : this.nTokenCnt), token, pose, i, j, r, n, ch;
while (this.msgFail.length == 0 && sDate.length > 0 && tokI < tokenC) {
token=tok[tokI].token;
this.SEPChars=this.FMTSEPChars;
if (tokI+1<tokenC && tok[tokI+1].type < 1 && (tok[tokI+1].token==hX_6._lZ || tok[tokI+1].token==hX_6._uZ))
this.SEPChars=this.FMTSEPChars.substr(1);
if (tok[tokI].type == hX_6._lX) {
} else if (tok[tokI].type >= 1) {
if (this.nStrict > 1 || tok[tokI].type == 1) {
if (token == sDate.substr(0,token.length)) {
sDate=sDate.substr(token.length);
} else {
if (this.nStrict <= 1) sDate=this.skipSeparators(sDate);
else {
this.msgFail=hX_6.L[this.plc].ERR_datetime_parse_fail;
this.intMsgFail=hX_6.INTERR_Parse_Literal + token;
break;
}
}
} else sDate=this.skipSeparators(sDate);
} else {
switch (token) {
case hX_6._lD:  case hX_6._l2D:
case hX_6._lH:	case hX_6._l2H:
case hX_6._uH:  case hX_6._u2H:
case hX_6._lK:  case hX_6._l2K:
case hX_6._uK:  case hX_6._u2K:
case hX_6._lM:  case hX_6._l2M:
case hX_6._lS:  case hX_6._l2S:
case hX_6._lW:  case hX_6._l2W:
case hX_6._uF:  case hX_6._uW:  case hX_6._lE:
if (this.nStrict <= 1)
if (this.skipIfMissing(sDate, true)) break;
n=this.matchDigits (sDate, 2, false);
if (n.fail) {
this.msgFail=hX_6.L[this.plc].ERR_datetime_parse_fail;
this.intMsgFail=hX_6.INTERR_Parse_TwoDig + token;
break;
}
sDate=n.s;
switch (tok[tokI].token) {
case hX_6._lD:  case hX_6._l2D:
intDay=n.n;
bNormalDate=true;
break;
case hX_6._lH:  case hX_6._l2H:
intHour=n.n;
bShortHour=true;
break;
case hX_6._uH:  case hX_6._u2H:
intHour=n.n;
break;
case hX_6._lK:  case hX_6._l2K:
intHour=n.n - 1;
break;
case hX_6._uK:  case hX_6._u2K:
intHour=n.n + 1;
bShortHour=true;
bKampm=true;
break;
case hX_6._lM:  case hX_6._l2M:
intMin=n.n; break;
case hX_6._lS:  case hX_6._l2S:
intSec=n.n; break;
case hX_6._uF:
case hX_6._lW:
case hX_6._uW:
case hX_6._l2W:  case hX_6._lE:
default:
break;
}
break;
case hX_6._uS:  case hX_6._u3S:  case hX_6._u2S:
case hX_6._uD:  case hX_6._u3D:  case hX_6._u2D:
if (this.nStrict <= 1)
if (this.skipIfMissing(sDate, true)) break;
n=this.matchDigits (sDate, 3, false);
if (n.fail) {
this.msgFail=hX_6.L[this.plc].ERR_datetime_parse_fail;
this.intMsgFail=hX_6.INTERR_Parse_TwoDig + token;
break;
}
sDate=n.s;
if (token.charAt(0) == hX_6._uD) {
bJulianDate=true;
intJDay=n.n;
} else {
intMS=n.n;
}
break;
case hX_6._lY:  case hX_6._l2Y:  case hX_6._l3Y:  case hX_6._l4Y:
case hX_6._uY:  case hX_6._u2Y:  case hX_6._u3Y:  case hX_6._u4Y:
n=this.matchDigits (sDate, 4, false);
if (n.fail) {
this.msgFail=hX_6.L[this.plc].ERR_datetime_parse_fail;
this.intMsgFail=hX_6.INTERR_Parse_TwoDig + token;
break;
}
if (token.charAt(0) != hX_6._uY) {
sDate=n.s;
intYear=n.n;
if (n.cnt <= 2) bShortYear=true;
bShortYearSpec=(token==hX_6._lY || token==hX_6._l2Y || token==hX_6._l3Y);
}
break;
case hX_6._uA:  case hX_6._u2A:  case hX_6._u3A:  case hX_6._u4A:  case hX_6._u5A:  case hX_6._u6A:  case hX_6._u7A:  case hX_6._u8A:
case hX_6._lG:  case hX_6._l2G:  case hX_6._l3G:  case hX_6._l4G:  case hX_6._l5G:  case hX_6._l6G:  case hX_6._l7G:  case hX_6._l8G:
if (this.nStrict <= 1)
if (this.skipIfMissing(sDate, true)) break;
n=this.matchDigits (sDate, 8, false);
if (n.fail) {
this.msgFail=hX_6.L[this.plc].ERR_datetime_parse_fail;
this.intMsgFail=hX_6.INTERR_Parse_TwoDig + token;
break;
}
if (token.charAt(0)==hX_6._uA) {
sDate=n.s;
intMSDay=n.n;
}
break;
case hX_6._uG:  case hX_6._u2G:
case hX_6._u3G: case hX_6._u4G:
if (this.nStrict <= 1)
if (this.skipIfMissing(sDate)) break;
r=false;
if (sDate.substr(0,2)==this._AD) ADtoCal=true;
for (i=0; i < this.descLbEpoch.length; i++) {
pose=((token.length <= 3) ? this.descLbEpoch[i][1] : this.descLbEpoch[i][0]).toUpperCase();
if ((sDate.substr(0, pose.length)).toUpperCase()==pose) {
sDate=sDate.substr(pose.length);
iEpoch=i;
r=true;
break;
}
pose=this.descLbEpoch[i][((token.length<=3)?1:0)].toUpperCase();
if (this.nStrict <= 1 && sDate.charAt(0).toUpperCase()==pose.charAt(0)) {
n=1;
while (n<sDate.length && n<pose.length && sDate.charAt(n).toUpperCase()==pose.charAt(n)) n++;
pose=pose.substr(0,n);
for (j=0; j<this.descLbEpoch.length; j++) {
if (j!=i && pose==this.descLbEpoch[j][((token.length <= 3)?1:0)].substr(0,n).toUpperCase()) j=99;
}
if (j<99) {
sDate=sDate.substr(n); j=0; ch=sDate.charAt(j);
while (j < sDate.length && ((ch==hX_6._P) || !this.matchDigit(ch) && !hX_6.isChar(ch,this.SEPChars))) {
j++; ch=sDate.charAt(j);
}
sDate=sDate.substr(j);
iEpoch=i;
r=true;
break;
}
}
}
if (!r) this.msgFail=hX_6.L[this.plc].ERR_epoch_name;
break;
case hX_6._uM:  case hX_6._u2M:  case hX_6._u3M:  case hX_6._u4M:
if (token.length <= 2 || this.nStrict <= 1) {
n=this.matchDigits (sDate, 2, false);
if (n.fail && this.nStrict > 1) {
this.msgFail=hX_6.L[this.plc].ERR_datetime_parse_fail;
this.intMsgFail=hX_6.INTERR_Parse_TwoDig + token;
break;
} else if (!n.fail) {
sDate=n.s;
intMonth=n.n;
if (intMonth>0 && ((token.length==3 && bNumberInShMonth) || (token.length>3 && bNumberInMonth))) {
pose=((token.length > 3)?hX_6.L[this.lc].STRA_DATE_MONTHS[this.epochIndex()][intMonth-1]:hX_6.L[this.lc].STRA_DATE_SHMONTHS[this.epochIndex()][intMonth-1]).toUpperCase();
i=pose.length-1;
if (!this.matchDigit(pose.substr(i,1))) {
while (i>=0 && !this.matchDigit(pose.substr(i-1,1))) i--;
while (sDate.length > 0 && i<pose.length && sDate.charAt(0)==pose.charAt(i)) {
sDate=sDate.substr(1);
i++;
}
}
}
break;
}
}
r=false;
for (i=0; i < hX_6.cal.monthsInYear(); i++) {
pose=((token.length > 3 || monthStrict <= 1)?hX_6.L[this.lc].STRA_DATE_MONTHS[this.epochIndex()][i]:hX_6.L[this.lc].STRA_DATE_SHMONTHS[this.epochIndex()][i]).toUpperCase();
if (sDate.substr(0, pose.length).toUpperCase() == pose) {
sDate=sDate.substr(pose.length);
intMonth=i+1;
r=true;
break;
}
pose=hX_6.L[this.lc].STRA_DATE_MONTHS[this.epochIndex()][i].toUpperCase();
if (monthStrict <= 1 && sDate.charAt(0).toUpperCase()==pose.charAt(0)) {
n=1;
while (n<sDate.length && n<pose.length && sDate.charAt(n).toUpperCase()==pose.charAt(n)) n++;
pose=pose.substr(0,n);
for (j=0; j<hX_6.cal.monthsInYear(); j++)
if (j!=i && pose==hX_6.L[this.lc].STRA_DATE_MONTHS[this.epochIndex()][j].substr(0,n).toUpperCase()) j=99;
if (j<99) {
sDate=sDate.substr(n); j=0; ch=sDate.charAt(j);
while (j < sDate.length && (!this.matchDigit(ch) && !hX_6.isChar(ch,this.SEPChars))) {
j++; ch=sDate.charAt(j);
}
sDate=sDate.substr(j);
intMonth=i+1;
r=true;
}
}
}
if (!r)	this.msgFail=hX_6.L[this.plc].ERR_month_name;
break;
case hX_6._uE:  case hX_6._u2E:
case hX_6._u3E: case hX_6._u4E:
if (this.nStrict <= 1)
if (this.skipIfMissing(sDate)) break;
r=false;
for (i=0; i<7; i++) {
pose=((token.length <= 3)?hX_6.L[this.lc].STRA_DATE_SHDAYS[this.epochIndex()][i]:hX_6.L[this.lc].STRA_DATE_DAYS[this.epochIndex()][i]).toUpperCase();
if (sDate.substr(0, pose.length).toUpperCase() == pose) {
sDate=sDate.substr(pose.length);
r=true;
break;
}
}
if (!r && this.nStrict <= 1) {
sDate=(token.length <= 3)?(this.skipToSeparator(sDate,(this.nStrict==0)?-1:hX_6.c.L[this.lc].MAX_STR_DATE_SHDAYS[this.epochIndex()])):(this.skipToSeparator(sDate,-1));
r=true;
}
if (!r) this.msgFail=hX_6.L[this.plc].ERR_day_name;
break;
case hX_6._lA:
if (this.nStrict <= 1)
if (this.skipIfMissing(sDate)) break;
if (sDate.length >= 2 && sDate.substr(0,2).toUpperCase() == this.sAM.toUpperCase()) {
ampm=this.sAM;
sDate=sDate.substr(2);
} else if (sDate.length >= 2 && sDate.substr(0,2).toUpperCase() == this.sPM.toUpperCase()) {
ampm=this.sPM;
sDate=sDate.substr(2);
} else {
this.msgFail=hX_6.L[this.plc].ERR_ampm_name;
}
break;
case hX_6._lZ: case hX_6._uZ:
this.SEPChars=this.FMTSEPChars.substr(1);
if (this.nStrict <= 1)
if (this.skipIfMissing(sDate)) break;
r=false;
zISO=(token.charAt(0)==hX_6._uZ);
if (!zISO || this.nStrict <= 1) {
for (i=0; i < hX_6.L[this.lc].STR_TIMEZONES.length; i++) {
if (hX_6.L[this.lc].STR_TIMEZONES[i][1] != null && hX_6.L[this.lc].STR_TIMEZONES[i][1].length > 0) {
pose=hX_6.L[this.lc].STR_TIMEZONES[i][1].toUpperCase();
if (sDate.substr(0, pose.length).toUpperCase() == pose) {
zOffset=this.readTimeZoneOffset(hX_6.L[this.lc].STR_TIMEZONES[i][0]);
sDate=sDate.substr(pose.length);
r=true;
break;
}
pose=hX_6.L[this.lc].STR_TIMEZONES[i][3].toUpperCase();
if (sDate.substr(0, pose.length).toUpperCase() == pose) {
zOffset=this.readTimeZoneOffset(hX_6.L[this.lc].STR_TIMEZONES[i][2]);
sDate=sDate.substr(pose.length);
r=true;
break;
}
}
}
}
if (zISO && !r) {
pose=sDate.charAt(0);
r=zISO=(this.matchDigit(pose) || pose == this._PLS || pose == this._MIN);
} else zISO=false;
if (r && (i==0 || zISO)) {
var sign=+1;
if (sDate.charAt(0) == this._PLS)
sDate=sDate.substr(1);
else if (sDate.charAt(0) == this._MIN) {
sign=-1;
sDate=sDate.substr(1);
}
n=this.matchDigits (sDate, 2, (this.nStrict>1));
r=!n.fail;
if (r) {
sDate=n.s;
if (n.n < 0 || n.n > 13) r=false;
else zOffset=sign * (n.n * 60);
if (r) {
if (sDate.charAt(0) == this.TZSeparator) sDate=sDate.substr(1);
n=this.matchDigits (sDate, 2, (this.nStrict>1));
r=!n.fail;
if (!r && this.nStrict <= 1) {
r=true;
n.s=0;
}
if (r) {
sDate=n.s;
if (n.n < 0 || n.n > 59)
r=false;
else
 zOffset=sign * (Math.abs(zOffset) + n.n);
}
}
}
}
if (!r) this.msgFail=hX_6.L[this.plc].ERR_tzone_name;
break;
default:
break;
}
}
tokI++;
}
if (this.msgFail.length == 0 && this.nStrict > 1 && (tokI != tokenC || sDate.length > 0)) {
this.msgFail=hX_6.L[this.plc].ERR_datetime_parse_fail;
this.intMsgFail=hX_6.INTERR_Parse_end + sDate;
}
if (ADtoCal) hX_6.cal.setCalendar(true, false, false);
if (this.msgFail.length == 0 && iEpoch!=null && this.isShortYr && iEpoch<this.descLbEpoch.length-2) {
if (this.nStrict > 1 && intYear>(1+this.descYrEpoch[iEpoch+1]-this.descYrEpoch[iEpoch]))
this.msgFail=hX_6.L[this.plc].ERR_epoch_range;
}
if (this.msgFail.length == 0) {
if (bShortYearSpec && !this.isShortYr && bShortYear)
intYear=hX_6.cal.twoYearToFourYear(intYear, curdate, this.getBaseYear(curdate));
if (this.bIsChristianEra)
intYear=(iEpoch==0)?(-intYear):intYear;
else if (this.bIsGregorian) {
iEpoch=(iEpoch!=null) ? iEpoch : this.descLbEpoch.length-1;
intYear+=this.descYrEpoch[iEpoch]-1;
}
if (intYear == 0 && !this.bIsChristianEra)
this.msgFail=hX_6.L[this.plc].ERR_epoch_range;
else {
if (bJulianDate && !bNormalDate) {
o=hX_6.cal.julianToMonthDay(intJDay, intYear);
intMonth=o.month+1;
intDay=o.date;
}
if (!bJulianDate && !bNormalDate) {
intDay=hX_6.cal.daysInMonth(intMonth-1,intYear);
if (zOffset) intDay=Math.min(intSvDay,intDay);
}
if (this.nStrict == 0) {
if(intMonth > hX_6.cal.monthsInYear(intYear) && intDay > 0 && intDay <= hX_6.cal.monthsInYear(intYear)) {
i=intMonth;
intMonth=intDay;
intDay=i;
}
}
}
}
if (this.msgFail.length == 0 && (intMonth < 1 || intMonth > hX_6.cal.monthsInYear(intYear)))
this.msgFail=hX_6.L[this.plc].ERR_month_range;
if (this.msgFail.length == 0 && (intDay < 1 || intDay > hX_6.cal.minDaysInMonth())) {
i=hX_6.cal.daysInMonth(intMonth-1, intYear);
if (intDay < 1 || intDay > i) {
this.msgFail=hX_6.L[this.plc].ERR_month_rangelimit;
args[1]= hX_6.L[this.lc].STRA_DATE_MONTHS[this.epochIndex()][intMonth-1];
args[2]= i;
}
}
if (this.msgFail.length == 0) {
if (!bShortHour) {
if (this.nStrict <= 1 && ampm.length > 0) {
if (ampm == this.sPM && intHour <= 12)
intHour+=12;
}
if (intHour < 0 || intHour > 23) {
this.msgFail=hX_6.L[this.plc].ERR_hour_range_long;
}
} else {
if (this.nStrict <= 1) {
if (intHour > 12) {
ampm=this.sPM;
intHour -= 12;
}
if (ampm.length == 0)
ampm=(intHour >= 6 && intHour <= 11) ? this.sAM : this.sPM;
if (intHour==0 && !bKampm && ampm==this.sAM) intHour=12;
}
if (intHour < 1 || intHour > 12)
this.msgFail=hX_6.L[this.plc].ERR_hour_range_short;
if (bKampm) {
if (ampm == this.sPM && intHour != 0) {
intHour+=12;
}
else if (ampm == this.sAM && intHour == 0)
intHour=0;
} else {
if (ampm == this.sPM && intHour != 12)
intHour+=12;
else if (ampm == this.sAM && intHour == 12)
intHour=0;
}
}
}
if (this.msgFail.length == 0 && (intMin < 0 || intMin > 59))
this.msgFail=hX_6.L[this.plc].ERR_minute_range;
if (this.msgFail.length == 0 && (intSec < 0 || intSec > 59))
this.msgFail=hX_6.L[this.plc].ERR_second_range;
if (this.msgFail.length > 0) {
this.msgFail=hX_6.NLSFormatMsg (this.msgFail, args);
return null;
}
r=hX_6.cal.calendarToDate(intYear, intMonth-1, intDay, intHour, intMin, intSec, intMS);
if (zOffset && (zOffset !=  -(r.getTimezoneOffset()))) {
intMin=r.getMinutes() - (zOffset + r.getTimezoneOffset());
r=hX_6.cal.calendarToDate(intYear, intMonth-1, intDay, intHour, intMin, intSec, intMS);
}
return {value:r, token:(tokI > 0)?tokI-1:0, pos:(iDate.length-sDate.length)};
}
HxG_6.prototype.DateTimeConverter.prototype.stringToLastToken=function (iDate, pos) {
var r, token=0, all="hHkKms";
if (iDate.length <= 0 || pos == 0) {
while (token < this.sTokens.length && !(this.sTokens[token].type == 0 && hX_6.isChar(this.sTokens[token].token.charAt(0),all)))
token++;
} else {
r=this.stringToValueBase(iDate.substr(0,pos));
if (r) {
token=(r.token < this.sTokens.length) ? r.token : this.sTokens.length-1;
if (this.sTokens[token].type != 0) {
while (token < this.sTokens.length && this.sTokens[token].type != 0)
token++;
}
token=(token >= this.sTokens.length) ? this.sTokens.length-1:token;
while (token >= 0 && !(this.sTokens[token].type == 0 && hX_6.isChar(this.sTokens[token].token.charAt(0),all)))
token--;
}
}
if (token < 0 || token >= this.sTokens.length)
return hX_6._lH;
else if (this.sTokens[token].token.charAt(0) == hX_6._lM)
return hX_6._lM;
else if (this.sTokens[token].token.charAt(0) == hX_6._lS)
return hX_6._lS;
else
 return hX_6._lH;
}
HxG_6.prototype.DateTimeConverter.prototype.posFromLastToken=function (iDate, token) {
if (iDate.length==0 || !token || token.length!=1) return 0;
var r, i, hit=false, test=(token==hX_6._lS) ? hX_6._lS : ((token==hX_6._lM) ? hX_6._lM : "hHkK");
for (i=0; i < this.sTokens.length && !hit; i++) {
if (this.sTokens[i].type==0 && hX_6.isChar(this.sTokens[i].token.charAt(0),test))
hit=true;
}
if (!hit) return 0;
i=(i>0)?i-1:0;
r=this.stringToValueBase(iDate, i);
return ((r) ? r.pos : 0);
}
HxG_6.prototype.DateTimeConverter.prototype.getMaxLength=function () {
return this.DF_MaxLength;
}
HxG_6.prototype.DateTimeConverter.prototype.isMarker=function (c) {
return (this.markers.indexOf(c) >= 0);
}
HxG_6.prototype.DateTimeConverter.prototype.genAssistMasks=function(promptC) {
var m=hX_6._E, p=hX_6._E, d=hX_6._E, sep=true, i,t,x,y,noT;
if (promptC) this.promptChar=promptC;
for (i=0; i < this.nTokenCnt; i++) {
t=this.sTokens[i].token;
if (this.sTokens[i].type == 0) {
sep=true;
switch (t.charAt(0)) {
case hX_6._lY:
if (this.isShortYr) {
m=m + hX_6._l2Y;
p=p + this._ll;
d=d + hX_6._l2Y;
} else {
m=m + hX_6._l4Y;
p=p + this._llll;
d=d + hX_6._l4Y;
}
break;
case hX_6._uG:
m=m + hX_6._uG;
d=d + hX_6.padR(hX_6._uG, this.abbEpochLength, hX_6._uG);
if (this.isShortYr || this.bIsChristianEra)
p=p + ((this.minEpoch==2)?hX_6.padR(this._l+this._l,this.abbEpochLength,hX_6._B):hX_6.padR(this._l,this.abbEpochLength,hX_6._B));
else
 p=p + this.descLbEpoch[1][1];
noT=true;
break;
case hX_6._uM:
m=m + hX_6._u2M;
p=p + this._ll;
d=d + hX_6._l2M;
break;
case hX_6._lD:
m=m + hX_6._l2D;
p=p + this._ll;
d=d + hX_6._l2D;
break;
case hX_6._lH: case hX_6._uH:
m=m + hX_6._u2H;
p=p + this._ll;
d=d + hX_6._l2H;
break;
case hX_6._lM:
m=m + hX_6._l2M;
p=p + this._ll;
d=d + hX_6._u2A;
break;
case hX_6._lS:
m=m + hX_6._l2S;
p=p + this._ll;
d=d + hX_6._l2S;
break;
default:
sep=false;
break;
}
} else {
if (t.length > 0) {
m=m + ((sep) ? (hX_6._SQT+t+hX_6._SQT) : hX_6._E);
p=p + ((sep) ? t : hX_6._E);
d=d + ((sep) ? t : hX_6._E);
}
}
}
this.maskFormat=(noT)?hX_6.trimL(p):hX_6.Trim(p);
this.maskFormatDigits=hX_6.Trim(d);
m=hX_6.Trim(m);
if (m.length > 0) {
if (m.charAt(0) == hX_6._SQT) 		m=hX_6._SQT + hX_6.Trim(m.substr(1));
if (m.substr(0,2) == "''")			m=hX_6.Trim(m.substr(2));
if (m.charAt(m.length-1) == hX_6._SQT)	m=hX_6.Trim(m.substr(0, m.length-1)) + hX_6._SQT;
if (m.substr(m.length-2,2) == "''")	m=hX_6.Trim(m.substr(0,m.length-2));
}
this.simpleFormat=m;
var args=[];
args.push("strict:2");
if (this.simpleFormat)	args.push("format:"+this.simpleFormat);
if (this.base2DigitSpan)args.push("base-2digit-span:"+this.base2DigitSpan);
if (this.sEpoch)		args.push("epoch:"+this.sEpoch);
if (this.ICU4J)			args.push("icu4j:"+this.ICU4J);
if (this.digitFirst)	args.push("digits:" +this.digitFirst);
args.push("first-day-of-week:"+this.FDOW);
if (this.sAM) 			args.push("am:"+this.sAM);
if (this.sPM) 			args.push("pm:"+this.sPM);
if (this.locale)		args.push("locale:"+this.locale);
this.simpleConverter=new hX_6.DateTimeConverter(args[0],args[1],args[2],args[3],args[4],((args.length>5)?args[5]:null),((args.length>6)?args[6]:null),((args.length>7)?args[7]:null),((args.length>8)?args[8]:null),((args.length>9)?args[9]:null),((args.length>10)?args[10]:null));
this.simpleConverter.isSimple=true;
this.maskFormatFirst=-1;
this.maskFormatLast=-1;
for (i=0; i < this.maskFormat.length; i++) {
if (this.maskFormat.charAt(i) == hX_6._LB || this.maskFormat.charAt(i) == hX_6._QM) {
this.maskFormatLast=i;
this.maskFormatFirst=(this.maskFormatFirst >= 0) ? this.maskFormatFirst : i;
}
}
}
HxG_6.prototype.DateTimeConverter.prototype.getBaseYear=function(today) {
return (today.getFullYear() - this.base2DigitSpan);
}
HxG_6.prototype.DateTimeConverter.prototype.skipSeparators=function (str) {
var r=str;
while (r.length > 0 && hX_6.isChar (r.charAt(0), this.SEPChars)) r=r.substr(1);
return r;
}
HxG_6.prototype.DateTimeConverter.prototype.skipToSeparator=function (str, max) {
var i=this.countToSeparator(str, max);
return (str.substr(i));
}
HxG_6.prototype.DateTimeConverter.prototype.countToSeparator=function (str, max) {
var r=str, i=0;
while (r.length > 0 && (i < max || max < 0) && !hX_6.isChar (r.charAt(0), this.SEPChars)) {
r=r.substr(1);
i++;
}
return i;
}
HxG_6.prototype.DateTimeConverter.prototype.skipIfMissing=function (str, notdigit) {
var n, r=(hX_6.isChar (str.charAt(0), this.SEPChars));
if (!r) {
n=this.matchDigits (str, 1, true);
r=(notdigit) ? n.fail : (!n.fail);
}
return r;
}
HxG_6.prototype.DateTimeConverter.prototype.matchDigit=function (str) {
if (str.length > 0) {
if (hX_6.WDIGITS.indexOf(str.charAt(0)) >= 0) return true;
if (this.xDigits != null && this.xDigits.indexOf(str.charAt(0)) >= 0) return true;
}
return false;
}
HxG_6.prototype.DateTimeConverter.prototype.matchDigits=function (str, n, exact) {
var cnt=0, s=str, oN=hX_6._E, w, a;
while (cnt < n && s.length > 0) {
w=(hX_6.WDIGITS.indexOf(s.charAt(0)) >= 0);
a=(!w && this.xDigits != null && this.xDigits.indexOf(s.charAt(0)) >= 0);
if (!a && !w)
break;
else {
oN=oN + this.getDigit(s);
cnt++; s=s.substr(1);
}
}
return {fail:(cnt==0 || (exact && cnt!=n)), s:s, n:parseInt(oN,10), cnt:cnt};
}
HxG_6.prototype.DateTimeConverter.prototype.getDigit=function (s) {
if (this.xDigits == null || (hX_6.WDIGITS.indexOf(s.charAt(0)) >= 0) || (this.xDigits.indexOf(s.charAt(0)) < 0)) return (s.charAt(0));
return (hX_6.WDIGITS.charAt(s.charCodeAt(0) - this.xDigits.charCodeAt(0)));
}
HxG_6.prototype.DateTimeConverter.prototype.cvtToDigits=function (str) {
var s=str+hX_6._E, r=hX_6._E;
if (this.xDigits == null) return s;
while (s.length > 0) {
if (hX_6.WDIGITS.indexOf(s.charAt(0)) >= 0) r=r + this.xDigits.charAt(s.charCodeAt(0)-hX_6.WDIGITS.charCodeAt(0));
else r=r + s.charAt(0);
s=s.substr(1);
}
return r;
}
HxG_6.prototype.DateTimeConverter.prototype.digitTransmute=function (ev) {
if (this.xDigits != null) {
var c=hX_6.getChrCode(ev);
if (hX_6.WDIGITS.indexOf(c)>=0) hX_6.setChrCode(ev, this.xDigits.charAt(c.charCodeAt(0)-hX_6.WDIGITS.charCodeAt(0)));
}
}
HxG_6.prototype.DateTimeConverter.prototype.getZeroDigit=function () {
return (this.zeroDigit);
}
HxG_6.prototype.DateTimeConverter.prototype.readTimeZoneOffset=function(str) {
var sign=(str.charAt(0) == this._MIN) ? -1 : +1;
var zoffset=(parseInt(str.substr(1,2), 10)) * 60;
zoffset+=parseInt(str.substr(3,2), 10);
zoffset=zoffset * sign;
return zoffset;
}
HxG_6.prototype.DateTimeConverter.prototype.constantToValue=function (sDate) {
var oDate=null, len=sDate.length;
var i, yr, mo, dy, hr, mn, sc, z;
yr=mo=dy=mn=sc=0;
hr=z=null;
var bRelative=false, t, tstr=sDate.toUpperCase();
for (i=0; i < 4; i++) {
t=hX_6.STR_DATE_TODAY[i].toUpperCase();
if (tstr.length >= t.length && tstr.substr(0, t.length) == t) {
bRelative=true;
break;
}
}
if (bRelative) {
oDate=new Date();
oDate.setSeconds(0);
var daysAdj=0, dayAdj=(i==1) ? 1 : ((i==2) ? -1 : 0);
tstr=tstr.substr(t.length);
tstr=hX_6.allTrim(tstr);
if (tstr.charAt(0) == this._PLS) {
daysAdj=parseInt(tstr.substr(1),10);
} else if (tstr.charAt(0) == this._MIN) {
daysAdj=-(parseInt(tstr.substr(1),10));
}
if (i==3) {
oDate=hX_6.cal.adjustDate(oDate,0,0,daysAdj,0,0,0);
} else {
oDate.setHours(12);
oDate.setMinutes(0);
oDate=hX_6.cal.adjustDate(oDate,0,0,(dayAdj+daysAdj),0,0,0);
}
} else {
var iSign=+1;
if (sDate.charAt(0)==this._MIN) {
sDate=sDate.substr(1);
iSign=-1;
}
if (len >= 4) yr=(parseInt(sDate.substr(0,4), 10) * iSign) + ((iSign<0)?-1:0);
if (len >= 6) mo=parseInt(sDate.substr(4,2), 10);
if (len >= 8) dy=parseInt(sDate.substr(6,2), 10);
if (len >= 10) hr=parseInt(sDate.substr(8,2), 10);
if (len >= 12) mn=parseInt(sDate.substr(10,2), 10);
if (len >= 14) sc=parseInt(sDate.substr(12,2), 10);
if (len >= 16) z=parseInt(sDate.substr(14,3), 10);
if (yr == 0 && mo == 0 && dy == 0) {
oDate=new Date();
yr=oDate.getFullYear();
mo=oDate.getMonth()+1;
dy=oDate.getDate();
}
if (z) {
oDate=new Date(Date.UTC(yr, mo-1, dy, hr, mn, sc, 0));
oDate.setTime(oDate.getTime() - (z * 1000));
} else {
oDate=(hr==null)?(new Date (yr, mo-1, dy, 12, 0, 0)):(new Date (yr, mo-1, dy, hr, mn, sc));
}
}
return oDate;
}
HxG_6.prototype.DateTimeConverter.prototype.valueToConstant=function (oDate) {
var m=hX_6.padL(oDate.getMonth()+1, 2);
var d=hX_6.padL(oDate.getDate(), 2);
return (oDate.getFullYear() + m + d);
}
HxG_6.prototype.DateTimeConverter.prototype.nReplace=function (fmt, c, cc, p, x, n) {
var f=fmt, i=f.indexOf(cc), j;
while (i >= 0) {
j=i+2;
while (f.charAt(j)==c) j++;
f=hX_6.replaceAll(f, f.substring(i,j), hX_6.padL(this.cvtToDigits(x), ((n!=null)?n:(j-i-1)), p));
i=f.indexOf(cc);
}
return f;
}
HxG_6.prototype.DateTimeConverter.prototype.valueToString=function (oDate, ifmt, promptChar) {
this.msgFail=hX_6._E;
if (oDate == null) return hX_6._E;
var fmt=((ifmt != null) ? ifmt : this.sFormatTokenized), oddampm=(fmt.indexOf("%K") >= 0);
var o,x,y,z,h,k, pChar=((promptChar)?promptChar:this.getZeroDigit()), bDate=((ifmt!= null)?true:this.bHasDate), bTime=((ifmt!= null)?true:this.bHasTime);
this.setCalendar();
if (bDate) {
fmt=hX_6.replaceAll(fmt, "%MMMM", "@0");
fmt=hX_6.replaceAll(fmt, "%MMM",  "@1");
fmt=hX_6.replaceAll(fmt, "%EEEE", "@2");
fmt=hX_6.replaceAll(fmt, "%EEE",  "@3");
fmt=hX_6.replaceAll(fmt, "%EE",	"@3");
fmt=hX_6.replaceAll(fmt, "%E",	"@3");
fmt=hX_6.replaceAll(fmt, "%GGGG", "@4");
fmt=hX_6.replaceAll(fmt, "%GGG",  "@5");
fmt=hX_6.replaceAll(fmt, "%GG",   "@6");
fmt=hX_6.replaceAll(fmt, "%G",   	"@7");
}
if (bTime) {
fmt=hX_6.replaceAll(fmt, "%a",    "@8");
fmt=hX_6.replaceAll(fmt, "%z",    "@9");
fmt=hX_6.replaceAll(fmt, "%Z",    "@!");
}
if (bDate) {
o=this.valueToYearEpoch(oDate); y=o.year; z=(y==0)?pChar:this.getZeroDigit();
fmt=hX_6.replaceAll(fmt, "%yyyy",hX_6.padL(this.cvtToDigits(y), 4, z));
if (fmt.indexOf("%Y") >= 0) {
x=(hX_6.cal.getJulianWeek(oDate, this.FDOW)).yr;
fmt=hX_6.replaceAll(fmt, "%YYYY", hX_6.padL(this.cvtToDigits(x), 4, z));
}
if (fmt.indexOf("%y") >= 0 || fmt.indexOf("%Y") >= 0) {
if (this.isShortYr) {
fmt=this.nReplace(fmt,hX_6._lY,"%y",pChar,y);
if (fmt.indexOf("%Y") >= 0) fmt=this.nReplace(fmt,hX_6._uY,"%Y",pChar,x);
} else {
fmt=this.nReplace(fmt,hX_6._lY,"%y",pChar,(y%100),2);
if (fmt.indexOf("%Y") >= 0) fmt=this.nReplace(fmt,hX_6._uY,"%Y",pChar,(x%100),2);
}
}
fmt=this.nReplace(fmt,hX_6._uM,"%M",pChar,(o.month+1));
if (fmt.indexOf("%w") >= 0)	fmt=this.nReplace(fmt,hX_6._lW,"%w",pChar,hX_6.cal.getJulianWeek(oDate, this.FDOW).wk);
if (fmt.indexOf("%W") >= 0) fmt=this.nReplace(fmt,hX_6._uW,"%W",pChar,hX_6.cal.getJulianWeekOfMonth(oDate, this.FDOW));
fmt=this.nReplace(fmt,hX_6._lD,"%d",pChar,o.date);
if (fmt.indexOf("%D") >= 0)	fmt=this.nReplace(fmt,hX_6._uD,"%D",pChar,hX_6.cal.getJulianDay(oDate));
if (fmt.indexOf("%g") >= 0)	fmt=this.nReplace(fmt,hX_6._lG,"%g",pChar,hX_6.cal.gregorianToJulian(oDate));
if (fmt.indexOf("%F") >= 0) fmt=this.nReplace(fmt,hX_6._uF,"%F",pChar,hX_6.cal.getDayOfWeekInMonth(oDate));
if (fmt.indexOf("%e") >= 0) {
x=(hX_6.cal.getDay(oDate)-this.FDOW)+1;
x=(x<1)?x+7:x;
fmt=this.nReplace(fmt,hX_6._lE,"%e",pChar, x);
}
}
if (bTime) {
h=oDate.getHours();
k=this.cvtToDigits(oDate.getMilliseconds());
fmt=this.nReplace(fmt,hX_6._lH,"%h",pChar,(((h%12)==0)?12:(h%12)));
fmt=this.nReplace(fmt,hX_6._lK,"%k",pChar,(h+1));
fmt=this.nReplace(fmt,hX_6._uH,"%H",pChar,h);
fmt=this.nReplace(fmt,hX_6._uK,"%K",pChar,((((h%12)==0)?12:(h%12))-1));
fmt=this.nReplace(fmt,hX_6._lM,"%m",pChar,oDate.getMinutes());
fmt=this.nReplace(fmt,hX_6._lS,"%s",pChar,oDate.getSeconds());
fmt=hX_6.replaceAll(fmt, "%SSS", hX_6.padL(k, 3, pChar));
fmt=hX_6.replaceAll(fmt, "%SS",  hX_6.padL(k, 3, pChar).substr(0,2));
fmt=hX_6.replaceAll(fmt, "%S",   hX_6.padL(k, 3, pChar).substr(0,1));
fmt=this.nReplace(fmt,hX_6._uA,"%A", pChar, oDate.getMilliseconds() + (oDate.getSeconds()*1000) + (oDate.getMinutes()*60*1000) + (oDate.getHours()*60*60*1000));
}
if (bDate) {
fmt=hX_6.replaceAll(fmt, "@0", this.valueToMonthLabel(oDate));
fmt=hX_6.replaceAll(fmt, "@1", this.valueToShortMonthLabel(oDate));
fmt=hX_6.replaceAll(fmt, "@2", this.valueToDayLabel(oDate));
fmt=hX_6.replaceAll(fmt, "@3", this.valueToShortDayLabel(oDate));
fmt=hX_6.replaceAll(fmt, "@4", o.name);
fmt=hX_6.replaceAll(fmt, "@5", o.nameabb);
fmt=hX_6.replaceAll(fmt, "@6", o.nameabb);
fmt=hX_6.replaceAll(fmt, "@7", o.nameabb);
}
if (bTime) {
fmt=hX_6.replaceAll(fmt, "@8", this.valueToAmPmLabel(oDate, oddampm));
fmt=hX_6.replaceAll(fmt, "@9", this.valueToTimeZone(oDate));
fmt=hX_6.replaceAll(fmt, "@!", this.valueToTimeZone(oDate, true));
}
for (x=0; x < this.nLiteralCnt; x++)
fmt=hX_6.replaceAll(fmt, hX_6._UPC+x, this.sLiterals[x]);
return fmt;
}
HxG_6.prototype.DateTimeConverter.prototype.valueToAmPmLabel=function(oDate, oddampm) {
var o=oDate.getHours();
if (oddampm)
return (o==0) ? this.sPM : ((o <= 12) ? this.sAM : this.sPM);
else
 return (o < 12) ? this.sAM : this.sPM;
}
HxG_6.prototype.DateTimeConverter.prototype.valueToTimeZone=function(oDate, ISO) {
var r=hX_6._E, j, zoffset, k, z1, z2, z3, z4, dst=false, znames;
var z=-(oDate.getTimezoneOffset());
var feb=new Date(oDate.getFullYear(), 1, 7);
var jun=new Date(oDate.getFullYear(), 5, 15);
var zfeb=-(feb.getTimezoneOffset());
var zjun=-(jun.getTimezoneOffset());
if (z == zfeb && z != zjun)	dst=false;
if (z == zjun && z != zfeb)	dst=true;
z1=(dst) ? 2 : 0;
z2=(dst) ? 3 : 1;
znames=hX_6.L[this.lc].STR_TIMEZONES.length;
for (j=znames-1; !ISO && (j>0); j--) {
if (hX_6.L[this.lc].STR_TIMEZONES[j][0] == null || hX_6.L[this.lc].STR_TIMEZONES[j][0].length == 0) {
} else {
zoffset=this.readTimeZoneOffset(hX_6.L[this.lc].STR_TIMEZONES[j][z1]);
if (zoffset == z) {
r=hX_6.L[this.lc].STR_TIMEZONES[j][z2];
for (k=0;k<r.length;k++) {
if (this.matchDigit(r.charAt(k))) {
r=r.substr(0,k) + this.cvtToDigits(r.charAt(k)) + r.substr(k+1);
}
}
break;
}
}
}
if (r.length == 0) {
r=((ISO)?hX_6._E:hX_6.L[this.lc].STR_TIMEZONES[0][1]) + ((z < 0) ? this._MIN : this._PLS);
z=Math.abs(z);
var hr=Math.floor(z/60);
var mn=z-(hr*60);
hr=hX_6.padL(this.cvtToDigits(hr),2);
mn=hX_6.padL(this.cvtToDigits(mn),2);
r=r + hr + ((ISO)?hX_6._E:this.TZSeparator) + mn;
}
return (r);
}
HxG_6.prototype.DateTimeConverter.prototype.isDateFormat=function() {
return this.bHasDate;
}
HxG_6.prototype.DateTimeConverter.prototype.isTimeFormat=function() {
return this.bHasTime;
}
HxG_6.prototype.DateTimeConverter.prototype.setCalendar=function() {
hX_6.cal.setCalendar(this.bIsGregorian, this.bIsHebrew, this.bIsHijiri);
}
HxG_6.prototype.DateTimeConverter.prototype.valueToDayLabel=function(oDate) {
