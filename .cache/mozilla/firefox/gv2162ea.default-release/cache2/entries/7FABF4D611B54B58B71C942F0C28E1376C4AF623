this.sPM=hX_6.L[this.lc].STR_PM_SUFFIX;
this.base2DigitSpan=80;
this.DF_MaxLength=null;
this.xDigits=this.zeroDigit=null;
this.msgFail=this.intMsgFail=hX_6._E;
this.maskFormat=this.maskFormatDigits=this.maskFormatLast=this.maskFormatFirst=this.simpleFormat=this.simpleConverter=null;
this.isSimple=false;
this.markers=hX_6._LB + hX_6._QM;
this.promptChar=hX_6._E;
this._l="#"; this._ll="##"; this._lll="###"; this._llll="####";
this._MIN="-";
this._PLS="+";
this._AD=null;
hX_6.c.strInit(this.lc);
this.attributeTable=[];
this.attributeTable ['strict']=['nStrict',		hX_6.ARG_TO_NUM,	false,	0,	2];
this.attributeTable ['format']=['sFormat',		hX_6.ARG_TO_NAS,	false,	null,	null];
this.attributeTable ['base-2digit-span']=['base2DigitSpan',hX_6.ARG_TO_NUM,	false,	0,	99];
this.attributeTable ['epoch']=['sEpoch',		hX_6.ARG_TO_NAN,	false,	null,	null];
this.attributeTable ['icu4j']=['ICU4J', 		hX_6.ARG_IS_TRUE,	false,	null,	null];
this.attributeTable ['locale']=['locale', 		hX_6.ARG_TO_NAN,	false,	null,	null];
