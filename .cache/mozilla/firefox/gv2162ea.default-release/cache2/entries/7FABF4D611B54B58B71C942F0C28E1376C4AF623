this.sPM=hX_6.L[this.lc].STR_PM_SUFFIX;
this.base2DigitSpan=80;
this.DF_MaxLength=null;
this.xDigits=this.zeroDigit=null;
this.msgFail=this.intMsgFail=hX_6._E;
