y=hX_6.g.getAbsPosTop(pop.offsetParent);
pop.style.top=(poptop-y)+hX_6._PX;
y=hX_6.g.getAbsPosTop(pop);
}
if (x!=popleft){
x=hX_6.g.getAbsPosLeft(pop.offsetParent);
pop.style.left=(popleft-x)+hX_6._PX;
x=hX_6.g.getAbsPosLeft(pop);
}
}
popTopOff=parseInt(pop.style.top,10);
popLeftOff=parseInt(pop.style.left,10);
if (hX_6.brw.isIE() && pop.filters && pop.filters.length > 0) {
for (i=0; i < pop.filters.length; i++)
if (hX_6.isDef(typeof pop.filters[i].Apply)) pop.filters[i].Apply();
}
