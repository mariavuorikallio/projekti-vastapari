HxG_6.prototype.JSFargument=function (s) {
this.value=this.attribute=hX_6._E;
if (s && (hX_6.isString(s))) {
var q=s.indexOf(hX_6._CL);
q=(q < 0) ? s.indexOf(hX_6._EQ) : q;
if (q > 0) {
this.attribute=s.substring(0,q);
this.value=s.substring(q+1);
} else {
this.attribute=s;
}
this.attribute=hX_6.Trim(this.attribute);
this.attribute=this.attribute.toLowerCase();
}
}
HxG_6.prototype.ARG_IS_TRUE=1;
HxG_6.prototype.ARG_TO_NUM=2;
HxG_6.prototype.ARG_TO_NAN=3;
HxG_6.prototype.ARG_TO_FCN=4;
HxG_6.prototype.ARG_TO_FCNS= 5;
HxG_6.prototype.ARG_TO_NAS=6;
HxG_6.prototype.ARG_IS_TRUEV=7;
HxG_6.prototype.ARG_TO_BIGNUM=8;
HxG_6.prototype.ARG_TO_NAU=9;
HxG_6.prototype.ARG_TO_NAB=10;
HxG_6.prototype.JSFargument.prototype.parseArg=function (thisp, type, arg1, arg2) {
if (type != hX_6.ARG_TO_NAS)
this.value=hX_6.Trim(this.value);
if (type == hX_6.ARG_IS_TRUE)
return (!(this.value == hX_6._FLS || this.value == hX_6._OFF || this.value == hX_6._Z));
else if (type == hX_6.ARG_IS_TRUEV)
return ((this.value == hX_6._FLS || this.value == hX_6._OFF || this.value == hX_6._Z || this.value == hX_6._NO) ? hX_6._NO:hX_6._VS);
else if (type == hX_6.ARG_TO_NUM || type == hX_6.ARG_TO_BIGNUM) {
var num;
if (hX_6.isString(this.value)) {
if (type == hX_6.ARG_TO_NUM)
num=new Number(this.value);
else {
if (hX_6.c.BIGNUM_converter == null) hX_6.c.BIGNUM_converter=new hX_6.NumberConverter();
num=hX_6.c.BIGNUM_converter.constantToValue(this.value);
}
} else
 num=this.value;
num=Math.min(Math.max(arg1, num), arg2);
return num;
} else if (type == hX_6.ARG_TO_NAN || type == hX_6.ARG_TO_NAU ||type == hX_6.ARG_TO_NAS)
return ((this.value.length <= 0)?null:((type==hX_6.ARG_TO_NAU) ? this.value.toUpperCase():this.value));
else if (type == hX_6._ARG_TO_NAB)
return (this.value.length <= 0)?hX_6._E:this.value;
else if (type == hX_6.ARG_TO_FCN)
return eval(this.value);
else if (type == hX_6.ARG_TO_FCNS)
return (new Function ("event", this.value));
else
 return this.value;
}
HxG_6.prototype.parseArg=function (attributeTable, thisp, a) {
var i, j, v, v1, arg, desc, b=(hX_6.isString(a)) ? [a] : a;
for (i=0; i<b.length; i++) {
arg=new this.JSFargument(b[i]);
if (arg.attribute.length >= 0) {
desc=attributeTable[arg.attribute];
if (desc) {
if (!(desc[2]) || (desc[2] && !thisp.HTMLrendered)) {
v=(desc[1] == null) ? (arg.value) : (arg.parseArg(thisp, desc[1], desc[3], desc[4]));
if (thisp[desc[0]]!=null && hX_6.isArray(thisp[desc[0]])) {
while ((thisp[desc[0]]).length>0) (thisp[desc[0]]).pop();
j=(v==null) ? -1 : v.indexOf(hX_6._SM);
while (j >= 0) {
v1=v.substr(0,j);
v=v.substr(j+1);
(thisp[desc[0]]).push(v1);
j=(v==null) ? -1 : v.indexOf(hX_6._SM);
}
