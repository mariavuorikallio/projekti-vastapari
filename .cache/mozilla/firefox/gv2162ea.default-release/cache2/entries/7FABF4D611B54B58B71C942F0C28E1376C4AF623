}
HxG_6.prototype.JSFSubButton.prototype.dispatchBtnComponent=function(evt) {
if ((typeof hX_6)=="undefined") return true;
evt=(evt) ? evt : ((event) ? event: null);
if (evt && this) {
var src=((this.nodeType == hX_6.ELEMENT_NODE || this.nodeType == hX_6.DOCUMENT_NODE) ? this : this.parentNode);
if (src) {
var r, sibling, obj, skip=false, item=hX_6._E;
if (src.tagName==hX_6._IMG) src=src.parentNode;
if (src.tagName==hX_6._BTN || src.tagName==hX_6._DIV) {
if (src.tagName == hX_6._DIV) {
item="btn1";
if (src.parentNode.tagName==hX_6._DIV) src=src.parentNode;
if (src.previousSibling && src.previousSibling.tagName==hX_6._SPN) src=(src.previousSibling).previousSibling;
} else {
sibling=src.previousSibling;
src=src.parentNode;
if (src && src.tagName == hX_6._SPN) {
item=(sibling && sibling.tagName == hX_6._BTN)?"btn2":"btn1";
src=src.previousSibling;
}
}
if (src && src.id && src.tagName==hX_6._INP) {
if (evt.type==hX_6._evMCC) skip=true;
else if (evt.type==hX_6._evMC || evt.type==hX_6._evMD || evt.type==hX_6._evMU) skip=evt.button > 1;
if (!skip) {
obj=hX_6.getComponentById(src.id);
if (obj) {
r =obj.dispatchComponent (item, evt, evt.type, ((evt.keyCode)?evt.keyCode:null));
if (r!=null && r==false) hX_6.i.cancelAction(evt, true);
}
}
}
}
}
}
return r;
}
HxG_6.prototype.JSFSubButton.prototype.dispatchButtonMonitorResize=function(evt) {
evt=(evt) ? evt : ((event) ? event: null);
if (evt && this) {
