p=hX_6.g.getP(base.parentNode,hX_6._POS);
if (p.toLowerCase()==hX_6._REL || p.toLowerCase()=="static") base.parentNode.style.position="static";
}
if (hX_6.brw.isIE() && base.tagName && base.tagName==hX_6._TD) {
p=hX_6.g.getP(base,hX_6._POS);
if (p.toLowerCase()==hX_6._REL || p.toLowerCase()=="static") base.style.position="static";
}
}
tagtop=hX_6.g.getAbsPosTop(base); tagleft=hX_6.g.getAbsPosLeft(base);
if (dir != null && dir == hX_6.POP_DIR_RIGHT) {
poptop=tagtop - 1;
popleft=tagleft + tagwidth - 1;
} else {
poptop=tagtop + tagheight - 1;
popleft=tagleft;
}
}
pop.style.top=poptop + hX_6._PX;
pop.style.left= popleft + hX_6._PX;
pop.style.display=hX_6._BLK;
pop.style.visibility=hX_6._HI;
var x, y, h=hX_6.g.getRendHeight(pop), w=hX_6.g.getRendWidth(pop), popTopOff, popLeftOff;
var winheight=hX_6.g.getWindowClientHeight(), winwidth=hX_6.g.getWindowClientWidth();
var wintop=Math.max(document.body.scrollTop,document.body.parentNode.scrollTop),
winleft=Math.max(document.body.scrollLeft,document.body.parentNode.scrollLeft);
if (((poptop-wintop) + h > winheight) && (winheight > h)) {
if ((tagtop-h+1)>=0) pop.style.top=(tagtop - h + 1) + hX_6._PX;
}
if (dir != null && dir==hX_6.POP_DIR_CENTER) {
popleft+=((tagwidth - w)/2);
pop.style.left= popleft + hX_6._PX;
}
