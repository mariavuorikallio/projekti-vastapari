HxG_6.prototype.DateTimeValidator.prototype.setAttribute=function(a) {
var wasmax=this.maximumBound, wasmin=this.minimumBound;
if (hX_6.c.STDDTP_converter==null) hX_6.c.STDDTP_converter=new hX_6.DateTimeConverter();
