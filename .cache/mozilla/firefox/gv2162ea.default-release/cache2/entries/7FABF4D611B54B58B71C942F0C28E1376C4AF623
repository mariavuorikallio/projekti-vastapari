HxG_6.prototype.StringValidator=function () {
this.type=hX_6.CVT_string;
this.lc=hX_6.currentLocale();
this.minimumLength=this.maximumLength=this.required=null;
this.constraint=this.constraintImp=null;
this.constraintExp=this.constraintExpImp =null;
this.ELexpression=this.ELexpressionImp=null;
this.msgFail=hX_6._E;
this.attributeTable=[];
this.attributeTable ['required']=['required',		hX_6.ARG_IS_TRUE,	false,	null, null];
this.attributeTable	['minimum-length']=['minimumLength',	hX_6.ARG_TO_NUM,	false,	0, 9999999];
this.attributeTable	['min-length']=['minimumLength',	hX_6.ARG_TO_NUM,	false,	0, 9999999];
this.attributeTable	['maximum-length']=['maximumLength',	hX_6.ARG_TO_NUM,	false,	1, 9999999];
this.attributeTable	['max-length']=['maximumLength',	hX_6.ARG_TO_NUM,	false,	1, 9999999];
this.attributeTable	['constraint']=['constraint',	hX_6.ARG_TO_NAN,	false,	null, null];
this.attributeTable	['constraint-regexpression']= ['constraintExp',	hX_6.ARG_TO_NAN,	false,	null, null];
this.attributeTable	['constraint-expression']=['ELexpression',	hX_6.ARG_TO_NAN,	false,	null, null];
this.setAttribute(arguments);
}
HxG_6.prototype.StringValidator.prototype.setAttribute=function(a) {
hX_6.parseArg (this.attributeTable, this, a);
if (this.minimumLength != null && this.minimumLength < 0) this.minimumLength=0;
if (this.maximumLength != null && this.maximumLength < 1) this.maximumLength=1;
if (this.maximumLength && this.minimumLength && this.minimumLength >= this.maximumLength) this.minimumLength=this.maximumLength=null;
if (this.constraint) this.constraintImp=hX_6.regExp.getRegExpByName(this.constraint);
if (this.constraintExp) this.constraintExpImp=hX_6.regExp.registerConstraint(this.constraintExp);
if (this.ELexpression != null) this.ELexpressionImp=hX_6.regExp.registerELConstraint(this.ELexpression);
return true;
}
HxG_6.prototype.StringValidator.prototype.getAttribute=function (attribute) {
return (hX_6.getArg(this.attributeTable, this, attribute));
}
HxG_6.prototype.StringValidator.prototype.lastError=function () {
return ((this.msgFail && this.msgFail.length > 0) ? this.msgFail : null);
}
HxG_6.prototype.StringValidator.prototype.validate=function (teststr, iC) {
this.msgFail=hX_6._E;
var C=(iC != null) ? hX_6.getConverterById(iC) : null;
if (teststr != null && !hX_6.isString(teststr)) {
this.msgFail=hX_6.L[this.lc].ERR_required;
return false;
}
var test=(teststr == null) ? hX_6._E : teststr;
test=test.replace(/[\r\n]/g, hX_6._E);
if (test.length == 0) {
if (this.required) {
this.msgFail=hX_6.L[this.lc].ERR_required;
return false;
} else
 return true;
}
if (this.minimumLength && test.length < this.minimumLength) {
this.msgFail=hX_6.L[this.lc].ERR_minimum;
return false;
}
if (this.maximumLength && test.length > this.maximumLength) {
this.msgFail=hX_6.L[this.lc].ERR_maximum;
return false;
}
if (this.constraintImp != null) {
if (!hX_6.regExp.testRegExp(test, this.constraintImp)) {
this.msgFail=hX_6.L[this.lc].ERR_constraint;
return false;
}
}
if (this.constraintExpImp != null) {
if (!hX_6.regExp.testRegExp(test, this.constraintExpImp)) {
this.msgFail=hX_6.L[this.lc].ERR_constraint;
return false;
}
}
if (this.ELexpressionImp != null) {
if (!hX_6.regExp.testELExp(test, this.ELexpressionImp)) {
this.msgFail=hX_6.L[this.lc].ERR_constraint;
return false;
}
}
return true;
}
HxG_6.prototype.MaskAssist=function (_elem, _state, _converter, _validator, _promptChar) {
if (!_elem || !_converter || !_state) return;
this.lc=hX_6.currentLocale();
this.isFilled=isFilled;
this.onFocus=onFocus;
this.onBlur=onBlur;
this.onClick=onClick;
this.onCut=onCut;
this.onPaste=onPaste;
this.onChangeKeyCode= onChangeKeyCode;
this.onChangeChrCode= onChangeChrCode;
this.onValidate=onValidate;
this.destroy=destroy;
var elem=_elem;
var state=_state;
var cvt=_converter;
var validator=_validator;
var promptChar=_promptChar;
this.getMask=function getMask() { return ((cvt.maskNFormat!=null)?cvt.maskNFormat:cvt.maskFormat); }
this.getMaskLast=function getMaskLast() { return ((cvt.maskNFormat!=null)?cvt.maskNFormatLast:cvt.maskFormatLast); }
this.getMaskFirst=function getMaskFirst() { return ((cvt.maskNFormat!=null)?cvt.maskNFormatFirst:cvt.maskFormatFirst); }
var handler=null;
handler=new hX_6.MaskAssist.prototype.maskHandler(elem, state, cvt, promptChar);
function destroy() {
if (handler!=null) {
if (handler.destroy) handler.destroy();
delete handler;
}
}
function isFilled (curpos) {
var c, v=hX_6.Trim(elem.value), l=v.length;
if (l < (this.getMask()).length)
return false;
c=v.charAt(this.getMaskLast());
return ((curpos > this.getMaskLast()) && (c != promptChar) && (c != hX_6._B));
}
function onFocus () {
var newValue=(elem.value) ? hX_6.trimR(elem.value) : null;
newValue=(newValue && newValue.length > 0) ? cvt.stringToValue(newValue, promptChar, true) : null;
newValue=(newValue) ? cvt.valueToString(newValue, promptChar) : handler.getEmptyMask();
elem.value=newValue;
handler.rebuildMask(newValue);
hX_6.sel.setCaretPos(elem, this.getMaskFirst(), null, hX_6.brw.isNavOrMoz());
return true;
}
function onBlur () {
var v=elem.value;
if (v.length==0) return true;
var newValue=cvt.stringToValue(v, promptChar);
if (newValue != null)
newValue=(newValue.length==0) ? hX_6._E : cvt.valueToString(newValue);
if (newValue != null) {
var m=handler.getEmptyMask();
m=hX_6.replaceAll(m,promptChar,hX_6._B);
if (m!=newValue)
elem.value=newValue;
else
 elem.value=hX_6._E;
return true;
}
return false;
}
function onClick (evt) {
return handler.onClick(evt);
}
function onCut (evt) {
return handler.onCut(evt);
}
function onPaste (evt) {
return handler.onPaste(evt);
}
function onChangeKeyCode (evt) {
return handler.onChangeKeyCode (evt);
}
function onChangeChrCode (evt) {
return handler.onChangeChrCode (evt);
}
function onValidate () {
var newValue=cvt.stringToValue(state.getValue(), promptChar);
if (newValue != null)
newValue=cvt.valueToString(newValue, promptChar);
if (newValue != null)
return true;
return false;
}
}
HxG_6.prototype.MaskAssist.prototype.maskHandler=function(_element, _state, _converter, _promptChar) {
var elem=_element;
var state=_state;
var cvt=_converter;
var promptChar=_promptChar;
var emptyMask=null;
this.onClick=onClick;
this.onCut=onCut;
this.onPaste=onPaste;
this.onChangeKeyCode= onChangeKeyCode;
this.onChangeChrCode= onChangeChrCode;
this.getEmptyMask=function getEmptyMask() {return emptyMask;}
this.rebuildMask=function rebuildMask(v) {if (cvt.rebuildProtectMask) cvt.rebuildProtectMask(v); }
var getKeyCode=hX_6.getKeyCode;
var getMask=function getMask() { return ((cvt.maskNFormat!=null)?cvt.maskNFormat:cvt.maskFormat); }
var getCChar=function (i) 	{ return (i < 0 || i >= (getMask()).length) ? hX_6._E : (getMask()).charAt(i); }
emptyMask=hX_6.replaceAll(cvt.maskFormat, hX_6._LB, promptChar);
for (var i=1; i<cvt.markers.length; i++)
emptyMask=hX_6.replaceAll(emptyMask, cvt.markers.charAt(i), promptChar);
function doDelete (evt) {
var v, s, cR, pos=state.getPos();
pos=(evt.type==hX_6._evCT) ? (pos) : ((evt.keyCode==hX_6.KEY_BS) ? pos-1 : pos);
if (pos < 0 && !state.hasSel())
return true;
cR=getCChar(pos);
if (!cvt.isMarker(cR) && cR != hX_6._EQ && !state.hasSel()) {
if (evt.type!=hX_6._evCT) {
pos=(evt.keyCode==hX_6.KEY_BS) ? (pos) : (pos+1);
state.setPos(pos);
return -2;
}
return false;
}
v=state.getValue(); s=state.hasSel();
if (s) {
var rg=hX_6.sel.getSelectionR(elem);
state.setValue((rg.start <= 0 && rg.end >= v.length-1)?emptyMask:fillWithMaskPiece(v, rg.start, rg.end));
} else
 if (pos>=0 && pos<v.length) state.setValue (v.substr(0,pos) + promptChar + v.substr(pos+1));
pos=(s || (evt.type!=hX_6._evCT && evt.keyCode==hX_6.KEY_BS)) ? pos : pos+1;
state.setPos(pos);
return -2;
}
function doInsert (v, chi, pos, sel) {
var ch=(chi==hX_6._B)?promptChar:chi;
if (sel) {
var rg=hX_6.sel.getSelectionR(elem);
v=fillWithMaskPiece(v, rg.start, rg.end);
var i, l=v.length;
for (i=rg.start; i < rg.end; i++) {
if (cvt.isMarker(getCChar(i))) break;
}
if (i < rg.end)
v=v.substr(0,i) + ch + v.substr(i+1);
state.setPos(rg.start+1);
} else {
v=v.substr(0,pos) + ((ch==hX_6._B)?promptChar:ch) + v.substr(pos+1);
pos=getMoveForward (v, pos+1);
state.setPos(pos);
}
state.setValue(v);
return -2;
}
function doCaretPrev (evt) {
if (evt.shiftKey || state.hasSel())
return -1;
else {
var pos=getMovePrevious (state.getValue(), state.getPos()-1);
state.setPos(pos);
return -2;
}
}
function doCaretNext (evt) {
if (evt.shiftKey || state.hasSel())
return -1;
else {
var pos=getMoveForward (state.getValue(), state.getPos()+1);
state.setPos(pos);
return -2;
}
}
function getMovePrevious (v, pos) {
var i, c, c1, l=v.length;
pos=(pos < 0) ? 0 : ((pos > l) ? l : pos);
for (i=pos; i > 0; i--) {
c=getCChar(i);
c1=getCChar(i-1);
if (cvt.isMarker(c) || cvt.isMarker(c1))
return (i);
}
return (0);
}
function getMoveForward(v, pos) {
var i, l=v.length;
pos=(pos < 0) ? 0 : ((pos > l) ? l : pos);
for (i=pos; i <= l; i++) {
if (cvt.isMarker(getCChar(i))) return (i);
}
return (l);
}
function  fillWithMaskPiece (v, start, end) {
return (v.substr(0,start) + emptyMask.substr(start, end-start) + v.substr(end));
}
function onClick(evt) {
if (state.hasSel())
return true;
var v=state.getValue(), pos=state.getPos();
if (pos <= 0 || pos > (v.length-1))
return true;
var cL=getCChar(pos-1), cR=getCChar(pos);
if (cvt.isMarker(cL) || cvt.isMarker(cL))
return true;
var MoveR=true, posLeft=getMovePrevious (v, pos), posRight=getMoveForward(v, pos);
if ((pos-posLeft) < (posRight-pos))
state.setPos(posLeft);
else
 state.setPos(posRight);
return -2;
}
function onChangeKeyCode (evt) {
switch (hX_6.getKeyCode(evt)) {
case hX_6.KEY_BS:
case hX_6.KEY_DEL:
return doDelete(evt);
case hX_6.KEY_LTARROW:
return doCaretPrev(evt);
case hX_6.KEY_RTARROW:
return doCaretNext(evt);
}
return true;
}
function onCut(evt) {
if (hX_6.brw.isIE() && state.hasSel()) {
var cutText=hX_6.sel.getSelection(elem);
window.clipboardData.setData("Text", cutText);
return doDelete(evt);
}
return true;
}
function onPaste (evt) {
if (hX_6.brw.isIE()) {
var cutText=window.clipboardData.getData("Text");
var v=state.getValue(), pos=state.getPos(), sel=state.hasSel();
if (sel) {
var rg=hX_6.sel.getSelectionR(elem);
v=(rg.start <= 0 && rg.end >= v.length-1)?emptyMask:fillWithMaskPiece(v, rg.start, rg.end);
}
var mChars=false, c, m, i, r, l=cutText.length;
for (i=0; i < l; i++) {
m=getCChar(i);
if (!cvt.isMarker(m)) {
mChars=(m == cutText.charAt(i));
break;
}
}
for (i=0; i < l; i++) {
m=getCChar(i);
c=cutText.charAt(i);
if (!cvt.isMarker(m) && m == c) {
} else {
c=(c==hX_6._B)?promptChar:c;
v=v.substr(0,pos) + c + v.substr(pos+1);
}
pos=(mChars) ? pos+1 : getMoveForward (v, pos+1);
}
state.setValue(v);
pos=getMoveForward (v, pos);
state.setPos(pos);
return -2;
}
return true;
}
function onChangeChrCode (evt) {
var v=state.getValue(), pos=state.getPos(), sel=state.hasSel(), ch=hX_6.getChrCode(evt);
if (evt.keyCode == hX_6.KEY_TAB || evt.keyCode == hX_6.KEY_ENTER)
return +2;
return (doInsert(v, ch, pos, sel));
}
}
HxG_6.prototype.POP_STYLE_DRAG=0;
HxG_6.prototype.POP_STYLE_CLICK= 1;
HxG_6.prototype.POP_STYLE_MANAGED= 2;
HxG_6.prototype.POP_STYLE_CLICK_SCROLLABLE= 3;
HxG_6.prototype.POP_DIR_RIGHT=1;
HxG_6.prototype.POP_DIR_CENTER=2;
HxG_6.prototype.JSFPopup=function (parentid, ownerobj, style, altComp) {
this.type="popup";
this.parentId=parentid;
this.DOMobj=this.parentDOMobj=null;
this.parentCOMPobj=(altComp!=null && altComp.type!=undefined)?altComp:null;
this.ownerDoc=document;
this.ownerObj=(ownerobj != null) ? ownerobj : document.body;
this.clickStyle=(style != null) ? style : hX_6.POP_STYLE_CLICK;
this.fullStyle=(this.clickStyle==hX_6.POP_STYLE_MANAGED);
this.sticky=false;
this.hideArray=this.hideArrayV=null;
this.dropShadowWidth=this.dropShadowColor=null;
this.dropDivs=[];
this.transparency=this.transDiv=null;
this.getElem=function getElem() { return this.DOMobj; }
this.getDoc=function getDoc()  { return this.ownerDoc; }
}
HxG_6.prototype.JSFPopup.prototype.onPageLoad=function() {
this.parentDOMobj=hX_6.getElementById(this.parentId);
this.parentCOMPobj=(this.parentCOMPobj!=null)?this.parentCOMPobj:hX_6.getComponentById(this.parentId);
if (this.parentDOMobj) {
if (!this.HTMLrendered) {
var elem=document.createElement(hX_6._DIV);
elem.setAttribute(hX_6._ID, this.id);
elem.setAttribute(hX_6._NAM, this.id + "_HXPOPUP");
elem.style.position=hX_6._ABS;
elem.style.left=elem.style.top=hX_6._ZPX;
elem.style.height=elem.style.width="10" + hX_6._PX;
elem.style.zIndex="1000000";
if (!this.fullStyle) elem.style.margin=elem.style.padding=hX_6._ZPX;
elem.style.verticalAlign=hX_6._TOP;
elem.style.overflow=hX_6._HI;
elem.style.display=hX_6._NO;
this.ownerObj.insertBefore (elem, (this.ownerObj.firstChild));
this.DOMobj=elem;
this.HTMLrendered=true;
return true;
}
}
return false;
}
HxG_6.prototype.JSFPopup.prototype.destroy=function() {
if (this.DOMobj) {
this.DOMobj.parentNode.removeChild(this.DOMobj);
delete this.DOMobj;
}
if (this.transDiv) {
this.transDiv.parentNode.removeChild(this.transDiv);
delete this.transDiv;
}
if (this.dropDivs) {
var e;
while (this.dropDivs.length > 0) {
e=this.dropDivs.pop();
e.parentNode.removeChild(e);
delete e;
}
}
if (this.hideArray) {
while (this.hideArray.length>0) this.hideArray.pop();
delete this.hideArray;
}
if (this.hideArrayV) {
while (this.hideArrayV.length>0) this.hideArrayV.pop();
delete this.hideArrayV;
}
return true;
}
HxG_6.prototype.JSFPopup.prototype.uivisible=function (dir, location, inline, absPt, overrideESC) {
var pop=this.DOMobj;
if (this.isPopped()) this.uirelease();
if (inline) {
if (pop) {
this.parentDOMobj=this.parentCOMobj=inline;
this.parentDOMobj.insertBefore(pop, this.parentDOMobj.firstChild);
pop.style.position="static";
pop.style.left=pop.style.top=hX_6._ZPX;
pop.style.visibility=hX_6._VS;
pop.style.display=hX_6._E;
}
} else {
if (pop && this.parentDOMobj) {
this.ownerObj.insertBefore (pop, this.ownerObj.firstChild);
var tagtop, tagleft, i, ds, off, poptop, popleft;
var base=(location != null) ? location : this.parentDOMobj;
var tagheight=hX_6.g.getRendHeight(base), tagwidth=hX_6.g.getRendWidth(base);
if (absPt) {
tagtop=poptop=absPt.Y; popleft=tagLeft=absPt.X;
} else {
if (hX_6.brw.isIE()) {
