this.locale=null;
this.nStrict=hX_6.DTFMT_DEFAULTSTRICT;
this.sFormat=hX_6.DTFMT_DATE0;
this.sFormatTokenized=null;
this.nLiteralCnt=this.nTokenCnt=0;
this.sLiterals=[];
this.sTokens=[];
