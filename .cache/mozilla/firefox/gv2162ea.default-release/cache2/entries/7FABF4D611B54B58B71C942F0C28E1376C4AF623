if (hX_6.brw.isNavOrMoz()) pop.style.opacity=hX_6._1;
this.addTransparency(ds, this.transparency);
}
var offX, offY, mgnTop, mgnLft;
if (hX_6.brw.isNavOrMoz()) {
pop.style.MozOutlineStyle=hX_6._SLD;
this.dropShadowWidth=parseInt(hX_6.g.getP(pop, "-moz-outline-width"),10);
